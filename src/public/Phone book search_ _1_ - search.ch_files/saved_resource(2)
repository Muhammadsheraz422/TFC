AppleID.auth.init({ clientId:'ch.search.login', scope:'name email', redirectURI:'https:' + U('//login/login/apple'), state:JSON.stringify(state), nonce:nonce });
apple_signin();
}).fail(function() {
alert("Failed to sign in with Apple.");
});
}
return false;
});
