L.Map.mergeOptions({
macosgesture: true,
zoomMultiplier: 0.2,
bounceAtGestureLimits: true
});
L.MacosGestureHandler = L.Handler.extend({
_macos_oldScrollWheelState: false,
addHooks: function () {
if (!("TouchEvent" in window) && "GestureEvent" in window) {
L.DomEvent.on(this._map._container, 'gesturestart gesturechange gestureend', this._onGesture, this);
}
},
removeHooks: function () {
L.DomEvent.off(this._map._container, 'gesturestart gesturechange gestureend', this._onGesture, this);
},
_onGesture: function (e) {
var map = this._map;
L.DomEvent.preventDefault(e);
switch (e.type) {
case 'gesturestart':
this._gesturezoom = map.getZoom();
break
case 'gesturechange':
this._zoom = this._gesturezoom * (1 - (1 - e.scale) * map.options.zoomMultiplier);
if (!map.options.bounceAtGestureLimits && ((this._zoom < map.getMinZoom() && e.scale < 1) || (this._zoom > map.getMaxZoom() && e.scale > 1))) {
this._zoom = map._limitZoom(this._zoom);
}
this._centerPoint = map.getSize()._divideBy(2);
if (map.options.macosgesture === 'center') {
this._center = map.containerPointToLatLng(this._centerPoint);
} else {
this._pinchPoint = map.mouseEventToContainerPoint(e);
this._centerLatLng = map.containerPointToLatLng(this._pinchPoint);
var delta = this._pinchPoint._subtract(this._centerPoint);
this._center = map.unproject(map.project(this._centerLatLng, this._zoom).subtract(delta), this._zoom);
}
L.Util.cancelAnimFrame(this._animRequest);
var moveFn = L.Util.bind(map._move, map, this._center, this._zoom, {pinch: true, round: false});
this._animRequest = L.Util.requestAnimFrame(moveFn, this, true);
break;
case 'gestureend':
L.Util.cancelAnimFrame(this._animRequest);
if (this._map.options.zoomAnimation) {
this._map._animateZoom(this._center, this._map._limitZoom(this._zoom), true, this._map.options.zoomSnap);
} else {
this._map._resetView(this._center, this._map._limitZoom(this._zoom));
}
break;
}
}
});
L.Map.addInitHook('addHandler', 'macosgesture', L.MacosGestureHandler);
L.Routing = L.Routing || {};
L.Routing.Waypoint = L.Class.extend(
{
initialize: function(latLng, name, options) {
this.latLng = L.latLng(latLng);
this.name = name;
if (options && 'proj_lat' in options && 'proj_lon' in options)
this.proj = L.latLng(options.proj_lat, options.proj_lon);
}
});
L.Routing.waypoint = function(latLng, name, options) {
return new L.Routing.Waypoint(latLng, name, options);
};
L.Routing.HotlineRenderer = L.Canvas.extend({
_updatePoly: function (layer) {
if (!this._drawing) { return; }
var parts = layer._parts;
var latlngs = L.LineUtil.isFlat(layer._latlngs) ? [layer._latlngs] : layer._latlngs;
var i,j;
if (!parts.length) { return; }
this._ctx.lineWidth = layer.options.weight + 2;
var sidewidth = 2 * layer.options.weight + 3;
this._ctx.lineCap = "round";
this._ctx.strokeStyle = '#000';
this._ctx.fillStyle = '#000';
for (i = 0;  i < parts.length; i++) {
this._ctx.beginPath();
for (j = 0; j < parts[i].length; j++) {
if (j == 0)
this._ctx.moveTo(parts[i][j].x, parts[i][j].y);
else if (!latlngs[i][j].is_point)
this._ctx.lineTo(parts[i][j].x, parts[i][j].y);
}
this._ctx.stroke();
}
for (i = 0;  i < parts.length; i++) {
for (j = 0; j < parts[i].length; j++) {
if (latlngs[i][j].is_point)
this._ctx.fillRect(parts[i][j].x - layer.options.weight - 2, parts[i][j].y - layer.options.weight - 2, sidewidth, sidewidth);
}
}
function gradient_color(value, max) {
var rel = Math.max(0, Math.min(max, value)) / max;
return rel < 0.5
? '#ff' + Math.round(rel *  2 * 255).toString(16).padStart(2, "0") + '00'
: '#' + Math.round((2 - rel * 2) * 255).toString(16).padStart(2, "0") + 'ff00';
}
this._ctx.lineWidth = layer.options.weight;
sidewidth = 2 * this._ctx.lineWidth + 1;
for (i = 0;  i < parts.length; i++) {
for (j = 1; j < parts[i].length; j++) {
this._ctx.strokeStyle = gradient_color(latlngs[i][j].speed, layer.options.maxspeed);
this._ctx.beginPath();
this._ctx.moveTo(parts[i][j - 1].x, parts[i][j - 1].y);
this._ctx.lineTo(parts[i][j].x, parts[i][j].y);
this._ctx.stroke();
}
}
for (i = 0;  i < parts.length; i++) {
for (j = 0; j < parts[i].length; j++) {
if (latlngs[i][j].is_point) {
this._ctx.fillStyle = gradient_color(Math.max(0, layer.options.maxtime - latlngs[i][j].timediff), layer.options.maxtime);
this._ctx.fillRect(parts[i][j].x - this._ctx.lineWidth - 1, parts[i][j].y - this._ctx.lineWidth - 1, sidewidth, sidewidth);
}
}
}
}
});
L.Routing.hotline_renderer = new L.Routing.HotlineRenderer();
L.Routing.Line = L.LayerGroup.extend(
{
includes: L.Evented.prototype,
options: {
extendToWaypoints: true,
},
initialize: function(route, options) {
L.setOptions(this, options);
this._env = options.env;
L.LayerGroup.prototype.initialize.call(this, options);
this._route = route;
this._hasMaxVia = route.waypoints.length >= route.env.max_vias + 2;
if (this.options.extendToWaypoints)
this._extendToWaypoints();
var last;
var dist  = 0;
this._dists = [];
for (var i = 0; i < route.segments.length; i++) {
var points = route.segments[i].points;
this._addSegment(
points,
route.segments[i].style || 'default',
true
);
for (var j = 0; j < points.length; j++) {
var p = points[j];
if (last)
dist += last.distanceTo(p);
this._dists.push([dist, p]);
last = p;
}
}
this._length = dist;
$('#map-overlay-left').on('mousemove', '.map-height-profile', L.bind(this._onShowPosition, this));
$('#map-overlay-left').on('touchmove', '.map-height-profile', L.bind(this._onShowPositionTouch, this));
$('#map-overlay-left').on('mouseout',  '.map-height-profile', L.bind(this._onRemovePosition, this));
},
onRemove: function (map) {
$('#map-overlay-left').off('mousemove', '.map-height-profile');
$('#map-overlay-left').off('mouseout',  '.map-height-profile');
this._onRemovePosition();
L.LayerGroup.prototype.onRemove.call(this, map);
},
addTo: function(map) {
map.addLayer(this);
return this;
},
getBounds: function() {
return L.latLngBounds(this._route.coordinates);
},
_findWaypointIndices: function() {
var wps = this._route.inputWaypoints,
indices = [],
i;
indices.push(0);
for (i = 1; i < wps.length - 1; i++)
indices.push(this._findClosestRoutePoint(wps[i].latLng));
indices.push(this._route.coordinates.length - 1);
return indices;
},
_findClosestRoutePoint: function(point) {
var minDist = Number.MAX_VALUE,
minIndex,
i,
d;
for (i = this._route.coordinates.length - 1; i >= 0 ; i--) {
d = point.distanceTo(this._route.coordinates[i]);
if (d < minDist) {
minIndex = i;
minDist = d;
}
}
return minIndex;
},
_extendToWaypoints: function() {
if (!this._route.coordinates.length)
return;
var wps = this._route.inputWaypoints,
wpIndices = this._getWaypointIndices(),
i,
wpLatLng,
routeCoord;
for (i = 0; i < wps.length; i++) {
wpLatLng = wps[i].latLng;
routeCoord = wps[i].proj ? wps[i].proj : L.latLng(this._route.coordinates[wpIndices[i]]);
this._addSegment([wpLatLng, routeCoord], 'direct');
}
},
_addSegment: function(coords, style, mouselistener) {
var i, max, min,
pl;
var variants = ["bg"];
if (style != "hotline")
variants.push("fg");
if (style == "direct")
variants.push("deco");
var maxspeed = this._env.center.mode == 'pedestrian' ? 10 : (this._env.center.mode == 'bicycle' ? 40 : 110);
var maxtime = 25;
maxspeed = maxspeed / 3.6
for (i = 0; i < variants.length; i++) {
var className = 'route-path route-path-' + variants[i] + ' route-path-' + style;
pl = L.polyline(coords, {noClip: style == "hotline", smoothFactor: style == "hotline" ? 0 : 1.0, stroke: true, weight: 5, className: className,  maxspeed: maxspeed, maxtime: maxtime, renderer: style == "hotline" ? L.Routing.hotline_renderer : null});
this.addLayer(pl);
if (mouselistener) {
pl.on('mousedown', this._onLineTouched, this);
pl.on('mousemove', function(e) { e.maxViaMsg = this._hasMaxVia; this._map.fire('route_tooltip', e);}, this);
pl.on('mouseout mousedown', function(e) { this._map.fire('route_tooltip_close', e);}, this);
}
}
},
_findNearestWpBefore: function(i) {
var wpIndices = this._getWaypointIndices(),
j = wpIndices.length - 1;
while (j >= 0 && wpIndices[j] > i) {
j--;
}
return j;
},
_onLineTouched: function(e) {
if (this._route.waypoints.length >= this._route.env.max_vias + 2)
return;
if ('search_poi_click' in this._map && this._map.search_poi_click._enabled && !('_route' in this._map && this._map._route._onDynPOI(e.latlng, this.options.l.get_zd()))) {
var map = this._map;
map.search_poi_click.disable();
window.setTimeout(
function() {
map.search_poi_click.enable();
},
400
);
}
var afterIndex = this._findNearestWpBefore(this._findClosestRoutePoint(e.latlng));
this.fire('linetouched', {
afterIndex: afterIndex,
latlng: e.latlng
});
},
_getWaypointIndices: function() {
if (!this._wpIndices) {
this._wpIndices = this._route.waypointIndices || this._findWaypointIndices();
}
return this._wpIndices;
},
_onShowPositionTouch: function (e) {
e.preventDefault();
var ev = e.originalEvent.changedTouches[0];
this._onShowPosition({pageX:ev.pageX, currentTarget:ev.target});
},
_binary_search: function(dists, fraction) {
var i = 0;
var j = dists.length - 1;
var target = fraction * dists[j][0];
while (i + 1 < j) {
var mid = (i+j+1)/2 | 0;
if (dists[mid][0] < target)
i = mid + 1;
else
j = mid;
}
if (dists[i][0] < target && i < dists.length - 1)
i++;
var cur = dists[i];
var latLng = cur[1];
if (i > 0 && cur[0] > target) {
var prev = dists[i - 1];
var a = (target - prev[0]) / (cur[0] - prev[0]);
latLng = new L.LatLng((1-a) * prev[1].lat + a * cur[1].lat, (1-a) * prev[1].lng + a * cur[1].lng, (1-a) * prev[1].alt + a * cur[1].alt);
}
return latLng;
},
_onShowPosition: function(e) {
if ($('.map-route-form:visible input[type="submit"].sl-spinner').length)
return;
var img = $(e.currentTarget);
var lo = img.data('leftoffset');
var ro = img.data('rightoffset');
var off = img.offset();
var x = e.pageX - off.left;
var cardoff = img.closest('.map-routeinfo').offset();
var pos = { top:off.top - cardoff.top, left:off.left - cardoff.left };
var height_profile_dists = img.data('dists');
if (!height_profile_dists) {
height_profile_dists = [];
var polyline = decodeURIComponent(img.attr('src').match(/polyline=([^&]*)&/)[1]);
var points = L.Routing.Route.prototype._polyline_decode(polyline, 0, 4);
var dist = 0;
for (var i = 0; i < points.length; i++) {
if (i)
dist += points[i-1].distanceTo(points[i]);
height_profile_dists.push([dist, points[i]]);
}
img.data('dists', height_profile_dists);
}
var left = Math.min(Math.max(pos.left, pos.left + x - 1), pos.left + img.width());
img.closest('.map-route-description').find('.map-charthover-bar').css({
left:left,
top:pos.top,
height:img.height()
}).show();
var p = (x - lo)/(img.width() - lo - ro);
if (p < 0) p = 0;
if (p > 1) p = 1;
var latLng  = this._binary_search(this._dists, p);
var alt = this._binary_search(height_profile_dists, p).alt;
if (img.parents('.map-route-summary.active').length) {
if (!this._positionMarker)
this._positionMarker = this._createPositionMarker(latLng).addTo(this._map);
else
this._positionMarker.setLatLng(latLng);
}
var tooltip = img.closest('.map-route-description').find('.map-charthover-tooltip');
var tooltip_title = tooltip.find('.title').text(Math.round(alt) + env.T.sl_altitude_unit);
tooltip.css('left', Math.max(0, left - tooltip_title.outerWidth() - 2)).show();
},
_onRemovePosition: function(e) {
$('.map-charthover-bar, .map-charthover-tooltip').hide();
if (this._positionMarker) {
this._map.removeLayer(this._positionMarker);
delete this._positionMarker;
}
},
_createPositionMarker: function(latLng) {
return L.circleMarker(latLng, {color:'black', weight:2, opacity:1, fill:true, fillColor:'white', fillOpacity:1, clickable:false}).setRadius(5);
}
});
L.Routing.line = function(route, options) {
return new L.Routing.Line(route, options);
};
L.Routing.Plan = L.Class.extend(
{
includes: L.Evented.prototype,
options: {
dragStyles: [
{color: 'black', opacity: 0.15, weight: 9},
{color: 'white', opacity: 0.8, weight: 6},
{color: 'red', opacity: 1, weight: 2, dashArray: '7,12'}
]
},
initialize: function(waypoints, options) {
L.Util.setOptions(this, options);
this._env = this.options.env;
this._waypoints = [];
this.setWaypoints(waypoints);
},
isReady: function() {
var i;
for (i = 0; i < this._waypoints.length; i++) {
if (!this._waypoints[i].latLng) {
return false;
}
}
return true;
},
getWaypoints: function() {
return this._waypoints.slice()
},
setWaypoints: function(waypoints) {
var args = [0, this._waypoints.length].concat(waypoints);
this.spliceWaypoints.apply(this, args);
return this;
},
spliceWaypoints: function() {
var args = [arguments[0], arguments[1]],
i;
for (i = 2; i < arguments.length; i++) {
args.push(arguments[i] && arguments[i].hasOwnProperty('latLng') ? arguments[i] : L.Routing.waypoint(arguments[i]));
}
[].splice.apply(this._waypoints, args);
while (this._waypoints.length < 2) {
this.spliceWaypoints(this._waypoints.length, 0, null);
}
this._updateMarkers();
this._fireChanged.apply(this, args);
},
onAdd: function(map) {
this._map = map;
this._updateMarkers();
},
onRemove: function() {
var i;
this._removeMarkers();
if (this._newWp) {
for (i = 0; i < this._newWp.lines.length; i++) {
this._map.removeLayer(this._newWp.lines[i]);
}
}
delete this._map;
},
_removeMarkers: function() {
var i;
if (this._markers) {
for (i = 0; i < this._markers.length; i++) {
if (this._markers[i]) {
this._map.removeLayer(this._markers[i]);
}
}
}
this._markers = [];
},
_createMarker: function(i, wp, n) {
var options = {
draggable: true,
riseOnHover:true,
icon: L.icon({
iconUrl: this._env.map_server + '/images/map_route_' + (!i ? 'start' : (i == n-1 ? 'end' : 'via')) + '.svg',
iconSize:    [25, 35],
iconAnchor:  [12, 35],
popupAnchor: [1, -34],
shadowSize:  [41, 41],
className: 'route-marker'
})
};
marker = L.marker(wp.latLng, options);
marker.on('mousemove', function(e) { this._map.fire('route_tooltip', e);}, this);
marker.on('mouseout mousedown', function(e) {this._map.fire('route_tooltip_close', e);}, this);
return marker;
},
_updateMarkers: function() {
var i,
m;
if (!this._map) {
return;
}
this._removeMarkers();
for (i = 0; i < this._waypoints.length; i++) {
if (this._waypoints[i].latLng) {
m = this._createMarker(i, this._waypoints[i], this._waypoints.length);
if (m) {
m.addTo(this._map);
this._hookWaypointEvents(m, i);
}
} else {
m = null;
}
this._markers.push(m);
}
},
_fireChanged: function() {
this.fire('waypointschanged', {waypoints: this.getWaypoints()});
if (arguments.length >= 2) {
this.fire('waypointsspliced', {
index: Array.prototype.shift.call(arguments),
nRemoved: Array.prototype.shift.call(arguments),
added: arguments
});
}
},
_hookWaypointEvents: function(m, i, trackMouseMove) {
var eventLatLng = function(e) {
return trackMouseMove ? e.latlng : e.target.getLatLng();
},
dragStart = L.bind(function(e) {
this.fire('waypointdragstart', {index: i, latlng: eventLatLng(e)});
}, this),
drag = L.bind(function(e) {
this._waypoints[i].latLng = eventLatLng(e);
this._waypoints[i].name = '';
this.fire('waypointdrag', {index: i, latlng: eventLatLng(e)});
}, this),
dragEnd = L.bind(function(e) {
this._waypoints[i].latLng = eventLatLng(e);
this._waypoints[i].name = '';
this.fire('waypointdragend', {index: i, latlng: eventLatLng(e)});
this._fireChanged();
}, this),
mouseMove,
mouseUp;
if (trackMouseMove) {
mouseMove = L.bind(function(e) {
this._markers[i].setLatLng(e.latlng);
drag(e);
}, this);
mouseUp = L.bind(function(e) {
this._map.dragging.enable();
this._map.off('mouseup', mouseUp);
this._map.off('mousemove', mouseMove);
dragEnd(e);
}, this);
this._map.dragging.disable();
this._map.on('mousemove', mouseMove);
this._map.on('mouseup', mouseUp);
dragStart({latlng: this._waypoints[i].latLng});
} else {
m.on('dragstart', dragStart);
m.on('drag', drag);
m.on('dragend', dragEnd);
m.on('click', L.bind(function(e) {
if (this._waypoints.length > 2) {
this.spliceWaypoints(i, 1);
this.fire('waypointremoved');
}
}, this));
}
},
dragNewWaypoint: function(e) {
var newWpIndex = e.afterIndex + 1;
this.spliceWaypoints(newWpIndex, 0, e.latlng);
this._hookWaypointEvents(this._markers[newWpIndex], newWpIndex, true);
}
});
L.Routing.plan = function(waypoints, options) {
return new L.Routing.Plan(waypoints, options);
};
L.Routing.Route = L.Class.extend(
{
includes: L.Evented.prototype,
options: {
routeDragInterval: 500,
waypointMode: 'connect',
showAlternatives: false
},
initialize: function(options) {
options = $.extend(options, this._route_env_options(options.env.center.route));
L.Util.setOptions(this, options);
this._env = this.options.env;
this._plan = this.options.plan || L.Routing.plan(this.options.waypoints, options);
this._plan.on('waypointschanged', this._onWaypointsChanged, this);
this._plan.on('waypointdragstart', this._onWaypointsDragStart, this);
this._plan.on('waypointdragend', this._onWaypointsDragEnd, this);
this._setupRouteDragging();
},
_update: function(p) {
var route = p.center.route
if (route) {
var args = {route: this._route_env_options(route)};
this._updateLines(args);
this._updatePOIs(route.pois);
} else {
this._clearLines();
}
if (p.updateForms)
this.options.l.stateupdate({center:p.center, noRouteUpdate:true, noFgUpdate:true, noCenterUpdate:true, history:true});
},
_route_env_options: function(route) {
var coordinates = [];
var segments = [];
var offset;
if (route.geometry) {
var geometry = route.geometry;
if (geometry[0] && typeof geometry[0][0] === 'string')
geometry = [geometry];
for (var i = 0; i < geometry.length; i++) {
offset = undefined;
for (var j = 0; j < geometry[i].length; j++) {
var path = geometry[i][j];
var points = this._polyline_decode(path[1], route.precision || 1, route.dimensions || 2, path[0] == "debug_default" ? null : offset, path[0] == "hotline");
var last = points.slice(-1)[0];
if (last) {//points can be empty
offset = _wgs2swiss(last);
offset.push(last.alt);
}
segments.push({style:path[0], points:points});
coordinates = coordinates.concat(points);
}
}
}
var wps = $.map(route.waypoints, function(p) { return L.Routing.waypoint(new L.LatLng(p.lat, p.lon), p.text, p);});
return {
env:            this._env,
segments:       segments,
coordinates:    coordinates,
inputWaypoints: wps,
waypoints:      wps
};
},
addTo: function (map) {
this.remove();
this._map = map;
this.onAdd(map);
map.on('route_tooltip',       this._onRouteTooltip, this)
map.on('route_tooltip_close', this._onRouteTooltipClose, this)
return this;
},
remove: function () {
if (!this._map) {
return this;
}
this.onRemove(this._map);
this._map = null;
return this;
},
onAdd: function(map) {
this._map = map;
this._plan.onAdd(this._map);
this._updateLines({route: this.options});
this._updatePOIs(this.options.env.center.route.pois);
},
onRemove: function(map) {
if (this._line) {
map.removeLayer(this._line);
}
map.removeLayer(this._plan);
},
getWaypoints: function() {
return this._plan.getWaypoints();
},
setWaypoints: function(waypoints) {
this._plan.setWaypoints(waypoints);
return this;
},
spliceWaypoints: function() {
var removed = this._plan.spliceWaypoints.apply(this._plan, arguments);
return removed;
},
_waypointsInViewport: function() {
var wps = this.getWaypoints(),
mapBounds,
i;
try {
mapBounds = this._map.getBounds();
} catch (e) {
return false;
}
for (i = 0; i < wps.length; i++) {
if (mapBounds.contains(wps[i].latLng)) {
return true;
}
}
return false;
},
_updateLines: function(routes) {
this._clearLines();
this._line = L.Routing.line(routes.route,
L.extend({
extendToWaypoints: this.options.waypointMode === 'connect',
env:this._env,
l:this.options.l
}, this.options.lineOptions)
);
this._line.addTo(this._map);
this._hookEvents(this._line);
},
_removePOIs: function() {
if (this.pois) {
$.each(this.pois, L.bind(function(id, poi) {
this.options.l.remove_poi(poi);
}, this));
}
},
_updatePOIs: function(pois) {
this._removePOIs();
this.pois = {};
if (pois)
$.each(pois, L.bind(function(id, data) {
data.id = id;
var poi = $.extend({}, data);
this.pois[id] = this.options.l.add_poi(poi);
}, this));
},
_hookEvents: function(l) {
l.on('linetouched', function(e) {
if (!this._onDynPOI(e.latlng, this.options.l.get_zd()))
this._plan.dragNewWaypoint(e);
}, this);
},
_onWaypointsChanged: function(e) {
if (!this._plan.isReady())
this._clearLines();
this.fire('waypointschanged', {waypoints: e.waypoints});
},
_onWaypointsDragStart: function(e) {
this._waypointdrag = true;
if ('search_poi_hover' in this._map && this._map.search_poi_hover._enabled) {
this._map.search_poi_hover.disable();
this._search_poi_hover_was_enabled = true;
}
},
_onWaypointsDragEnd: function(e) {
this._waypointdrag = false;
if (this._search_poi_hover_was_enabled)
this._map.search_poi_hover.enable();
},
_setupRouteDragging: function() {
var timer = 0,
waypoints;
this._plan.on('waypointdrag', L.bind(function(e) {
waypoints = e.waypoints;
if (!timer) {
timer = setTimeout(L.bind(function() {
this.route({waypoints: waypoints});
timer = undefined;
}, this), this.options.routeDragInterval);
}
}, this));
this._plan.on('waypointdragend', function() {
if (timer) {
clearTimeout(timer);
timer = undefined;
}
this.route({updateForms:true});
}, this);
this._plan.on('waypointremoved', function() {
this.route({updateForms:true});
}, this);
},
route: function(options) {
options = options || {};
if (this._plan.isReady()) {
var wps = options && options.waypoints || this._plan.getWaypoints();
this.fire('routingstart', {waypoints: wps});
var p = this._env.center.route;
if (p)
this._lastroute = this._env.center.route;
else
p = this._lastroute;
var params = $.extend({}, p.params, {
address: p.center,
points:  $.map(wps, function(p) { return p.name ? p.name : p.latLng.lat + ',' + p.latLng.lng }),
lang:    this._env.language
});
if (!options.updateForms)
params.geometry_only = 1;
var spinning_elements = options.updateForms ? $('.map-route-form:visible input[type="submit"], .map-info-card header .map-route-icon').addClass('sl-spinner') : undefined;
$('.map-info-card header .map-route-icon').addClass('sl-spinner');
this.options.l.get_center(
params,
L.bind(function(center) {
this._update({center:center, updateForms:options.updateForms});
if (spinning_elements)
spinning_elements.removeClass('sl-spinner');
}, this)
);
}
},
_clearLines: function() {
if (this._line) {
this._map.removeLayer(this._line);
delete this._line;
}
},
_polyline_decode: function(str, precision, tuple, offset, withspeed) {
var index = 0,
i = 0,
coordinates = [],
shift = 0,
result = 0,
byte = null,
point = [],
factor = Math.pow(10, typeof precision == "undefined" ? 1 : precision);
if (!tuple)
tuple = 2;
for (i = 0; i < tuple; i++)
point[i] = 0;
while (index < str.length) {
var latlon;
diff = [];
for (i = 0; i < tuple; i++) {
shift = 0; result = 0;
do {
byte = str.charCodeAt(index++) - 63;
result |= (byte & 0x1f) << shift;
shift += 5;
} while (byte >= 0x20);
diff[i] = ((result & 1) ? ~(result >> 1) : (result >> 1));
}
if (offset && coordinates.length == 0) {
latlon = _swiss2wgs(offset[0], offset[1]);
coordinates.push(new L.LatLng(latlon[0], latlon[1], offset[2]));
for (i = 0; i < tuple; i++)
point[i] = Math.round(offset[i] * factor);
}
for (i = 0; i < tuple; i++)
point[i] += diff[i];
var x = point[0] / factor;
var y = point[1] / factor;
var z = point[2] / factor;
latlon = _swiss2wgs(x, y);
var newlatlng = new L.LatLng(latlon[0], latlon[1], z);
if (withspeed) {
var length = Math.sqrt(diff[0]*diff[0] + diff[1]*diff[1]);
newlatlng.timediff = diff[3];
newlatlng.is_point = length == 0;
newlatlng.speed = length / diff[3];
}
coordinates.push(newlatlng);
}
if (withspeed)
coordinates[coordinates.length - 1].is_point = false;
return coordinates;
},
_onRouteTooltip: function(e) {
if (this._waypointdrag) {
this._onRouteTooltipClose();
return;
}
if (!this._tooltip)
this._tooltip = L.popup({
className:'map-tooltip map-poi-hover-tipp',
closeButton: false,
autoPan: false,
maxWidth: 400,
minWidth: 0,
closeOnClick: true,
offset: [0, 50]
});
var coord = new L.LatLng(e.latlng.lat, e.latlng.lng);
var zd = this.options.l.get_zd();
if (this._onDynPOI(coord, zd)) {
this._onRouteTooltipClose();
return;
}
this._last_hover_pos = coord;
var lastPos = this._tooltip.getLatLng();
if (lastPos && lastPos.distanceTo(coord) > 100 * zd)
this._tooltip.remove();
setTimeout(L.bind(function() {
if (!this._last_hover_pos || this._last_hover_pos.distanceTo(coord) > 10 * zd)
return;
if (this._map)
this._tooltip.setLatLng(this._last_hover_pos).setContent(e.maxViaMsg ? this._env.T.routedrag_maxvia : this._env.T.routedrag_tooltip).addTo(this._map)
}, this), 500);
},
_onRouteTooltipClose: function(e) {
delete this._last_hover_pos;
if (this._tooltip)
this._tooltip.remove();
},
_onDynPOI: function(coord, zd) {
var map = this.options.l;
var dynpois = map.dynpois;
if (dynpois) {
for (var i in dynpois) {
if (dynpois.hasOwnProperty(i)) {
var poi = dynpois[i];
if (coord.distanceTo(poi.latlng) <= Math.max(poi.offsetx, poi.offsety) * zd)
return true;
}
}
}
return false;
}
});
L.Routing.route = function(options) {
return new L.Routing.Route(options);
};
L.Map.include(
{
setRoute: function (env, l) {
if (this._route) {
this._route._plan._removeMarkers();
this._route._removePOIs();
this._route.remove();
}
if (env.center.route)
this._route = L.Routing.route({
env: env,
routeDragInterval: 100,
l: l
}).addTo(this);
return this;
}
});
(function () {
'use strict';
L.search_draw = L.search_draw || {};
L.search_draw.Mixin = L.search_draw.Mixin || {};
L.search_draw.Ellipse = L.Circle.extend({
type: 'Ellipse',
statics: {
boundsToParams: function(map, latLngBounds) {
var bounds = L.bounds(
map.latLngToLayerPoint(latLngBounds.getSouthWest()),
map.latLngToLayerPoint(latLngBounds.getNorthEast())
);
var c = bounds.getCenter();
var latlng = map.layerPointToLatLng(c);
var nw = latLngBounds.getNorthWest();
return {
latlng: latlng,
radiusX: latlng.distanceTo(L.latLng(latlng.lat, nw.lng)),
radiusY: latlng.distanceTo(L.latLng(nw.lat, latlng.lng))
};
},
},
initialize: function (f) {
this._f = f;
var p = f.properties;
var latlng = this._latlng = L.GeoJSON.coordsToLatLng(f.geometry.coordinates)
L.Circle.prototype.initialize.call(this, latlng, {radius:1, color: p.stroke, weight:p['stroke-width'], fill:false});
},
setBounds: function (latLngBounds) {
this._bounds = latLngBounds;
var p = L.search_draw.Ellipse.boundsToParams(this._map, latLngBounds);
this._latlng = p.latlng;
this._f.geometry.coordinates = L.GeoJSON.latLngToCoords(p.latlng);
this._f.properties.radiusX = p.radiusX;
this._f.properties.radiusY = p.radiusY;
return this.redraw();
},
_getBounds: function() {
var latlng = this._latlng ? this._latlng : L.GeoJSON.coordsToLatLng(this._f.geometry.coordinates);
var exactmpp = this._getExactmpp();
var c = this._map.latLngToLayerPoint(latlng);
var rx = this._f.properties.radiusX / exactmpp;
var ry = this._f.properties.radiusY / exactmpp;
return L.latLngBounds(
this._map.layerPointToLatLng(L.point(c.x - rx, c.y - ry)),
this._map.layerPointToLatLng(L.point(c.x + rx, c.y + ry))
);
},
_getExactmpp: function() {
var zoom = searchmap ? searchmap.get_zoom() : 11;
return (6378137.0 * 2 * Math.PI) / 512 / Math.pow(2, zoom) * Math.cos(this._latlng.lat * Math.PI / 180);
},
_project: function () {
var map = this._map;
this._point   = map.latLngToLayerPoint(this._latlng);
var exactmpp = this._getExactmpp();
this._radius  = this._f.properties.radiusX / exactmpp;
this._radiusY = this._f.properties.radiusY / exactmpp;
this._updateBounds();
},
_getCenterPositions: function () {
var corners = this._getCorners();
var positions = [];
for (var i = 0; i < corners.length; i++) {
var j = (i + 1) % corners.length;
positions.push(L.latLng(
(corners[i].lat + corners[j].lat) / 2,
(corners[i].lng + corners[j].lng) / 2
));
}
return positions;
},
_getCorners: function () {
var bounds = this._bounds,
nw = bounds.getNorthWest(),
ne = bounds.getNorthEast(),
se = bounds.getSouthEast(),
sw = bounds.getSouthWest();
return [sw, nw, ne, se];
},
});
L.search_draw.ellipse = function (f) {
return new L.search_draw.Ellipse(f);
};
L.search_draw.Path = L.Polyline.extend({
type: 'Path',
initialize: function (f) {
this._f = f;
var p = f.properties;
L.Polyline.prototype.initialize.call(this, L.GeoJSON.coordsToLatLngs(f.geometry.coordinates), {color: p.stroke, weight: p['stroke-width']});
},
setLatLngs: function (latlngs) {
L.Polyline.prototype.setLatLngs.call(this, latlngs);
this._f.geometry.coordinates = L.GeoJSON.latLngsToCoords(latlngs);
}
});
L.search_draw.path = function (f) {
return new L.search_draw.Path(f);
};
L.search_draw.Rectangle = L.Rectangle.extend({
type: 'Rectangle',
initialize: function(f) {
this._f = f;
var p = f.properties;
this._bounds = L.latLngBounds([
L.GeoJSON.coordsToLatLng(f.geometry.coordinates[0][0]),
L.GeoJSON.coordsToLatLng(f.geometry.coordinates[0][2])
]);
L.Rectangle.prototype.initialize.call(this, this._bounds, {color: p.stroke, weight: p['stroke-width'], fill:false});
},
_setLatLngs: function (latlngs) {
L.Rectangle.prototype._setLatLngs.call(this, latlngs);
this._bounds = new L.latLngBounds(latlngs[0], latlngs[2]);
},
_getCorners: function () {
var bounds = this._bounds,
nw = bounds.getNorthWest(),
ne = bounds.getNorthEast(),
se = bounds.getSouthEast(),
sw = bounds.getSouthWest();
return [sw, nw, ne, se];
},
setBounds: function (bounds) {
bounds = L.latLngBounds(bounds);
L.Rectangle.prototype.setBounds.call(this, bounds);
this._f.geometry.coordinates = L.search_draw.draw.Rectangle.coordinates(bounds);
}
});
L.search_draw.rectangle = function (f) {
return new L.search_draw.Rectangle(f);
};
L.search_draw.DynamicMarker = L.Marker.extend({
update: function () {
this.options.icon = this.myIcon();
this._initIcon();
L.Marker.prototype.update.call(this);
},
redraw: function () {
this.setIcon(this.myIcon())
}
});
L.search_draw.arrow_rotations = {r:0, d:90, l:180, u:270};
L.search_draw.Arrow = L.search_draw.DynamicMarker.extend({
type: 'Arrow',
initialize: function (f) {
this._f = f;
L.search_draw.DynamicMarker.prototype.initialize.call(this, L.GeoJSON.coordsToLatLng(f.geometry.coordinates), {icon: this.myIcon()});
},
myIcon: function () {
var x = 18,
y = 10,
zoom = searchmap ? searchmap.get_zoom() : 11,
p = this._f.properties;
var rotation = L.search_draw.arrow_rotations[p['marker-symbol']];
if (rotation) {
var offset_x = x - 10;
var offset_y = y - 10;
var rad = Math.PI / 180 * rotation;
var offset_x1 = Math.floor(offset_x * Math.cos(rad) - offset_y * Math.sin(rad));
var offset_y1 = Math.floor(offset_x * Math.sin(rad) + offset_y * Math.cos(rad));
x += (offset_x1 - offset_x);
y += (offset_y1 - offset_y);
}
var url = '/' + p.type + '.png?c=' + p.color.replace(/^#/, '');
if (rotation)
url += '&r=' + rotation;
if (zoom < 12)
url += '&m=small';
return L.icon({iconUrl: U(url), iconSize: [20, 20], iconAnchor: [x, y]});
}
});
L.search_draw.arrow = function (f) {
return new L.search_draw.Arrow(f);
};
L.search_draw.Poi = L.search_draw.DynamicMarker.extend({
type: 'Poi',
statics: {
color_map: {
'F4F4F4':'0',
'0C0C0C':'9',
'F4F400':'3',
'FF6600':'2',
'E90000':'1',
'E900E9':'8',
'CC66FF':'7',
'0000CC':'6',
'6BE900':'4',
'00FFFF':'5'
},
color_index: function (c) {
var color_map = this.color_map;
c = ('' + c).replace(/[^a-f0-9]/gi, '').toUpperCase();
if (color_map[c])
return color_map[c];
var idx = 0;
var r = parseInt(c.substr(0, 2), 16);
var g = parseInt(c.substr(2, 2), 16);
var b = parseInt(c.substr(4, 2), 16);
var d, diff = 3 * 255;
for(var color in color_map) {
d = Math.abs(r - parseInt(color.substr(0, 2), 16)) + Math.abs(g - parseInt(color.substr(2, 2), 16)) + Math.abs(b - parseInt(color.substr(4, 2), 16));
if (d < diff) {
diff = d;
idx = color_map[color];
}
}
return idx;
},
index_to_color: function(i) {
var inverse = {}
for (var c in this.color_map)
inverse[this.color_map[c]] = c;
var result = inverse[i];
if (!result)
result = inverse[1];
return result;
}
},
initialize: function (f) {
this._f = f;
L.search_draw.DynamicMarker.prototype.initialize.call(this,L.GeoJSON.coordsToLatLng(f.geometry.coordinates), {icon: this.myIcon()});
},
myIcon: function () {
var zoom = searchmap ? searchmap.get_zoom() : 11;
var color = this._f.properties['marker-symbol'],
size = zoom >= 17 ? 21 : ( zoom > 11 ? 17 : 7),
url = '/images/icons/' + size + '/p' + color + '.png';
return L.icon({iconUrl:U(url), iconSize:[size, size], iconAnchor:[Math.floor(size/2), Math.floor(size/2)]});
}
});
L.search_draw.poi = function (f) {
return new L.search_draw.Poi(f);
};
L.search_draw.Text = L.Marker.extend({
type: 'text',
initialize: function (f) {
this._f = f;
L.Marker.prototype.initialize.call(this, L.GeoJSON.coordsToLatLng(f.geometry.coordinates), {icon: this.createIcon()});
},
createIcon: function () {
var p = this._f.properties;
return L.divIcon({className:'', html:'<img src="' + U('/d/text?s=' + encodeURIComponent(p.title) + '&c=' + p.color.match(/[0-9a-zA-Z]+/)[0]) + '" alt="">', iconSize:[p.width, p.height], iconAnchor:[0, 0]});
},
redraw: function () {
this.setIcon(this.createIcon());
},
_setPos: function (pos) {
var p = this._f.properties;
var anchor = p['text-anchor'] || "";
pos = L.point(pos);
if (anchor.match('left'))
;
else if (anchor.match('right'))
pos = pos.add([-p.width, 0]);
else
pos = pos.add([-p.width/2, 0]);
if (anchor.match('top'))
;
else if (anchor.match('bottom'))
pos = pos.add([0, -p.height]);
else
pos = pos.add([0, -p.height/2]);
L.Marker.prototype._setPos.call(this, pos);
}
});
L.search_draw.text = function (f) {
return new L.search_draw.Text(f);
};
L.search_draw.DrawingPane = L.LayerGroup.extend({
addLayer: function (layer) {
layer.addEventParent(this);
return L.LayerGroup.prototype.addLayer.bind(this)(layer);
},
removeLayer: function (layer) {
layer.removeEventParent(this);
return L.LayerGroup.prototype.removeLayer.bind(this)(layer);
},
onRemove: function (map) {
for (var i in this._layers)
if (map.hasLayer(this._layers[i]))
map.removeLayer(this._layers[i]);
},
readGeoJSON: function(geojson) {
var features = geojson.features;
for (var i = 0; i < features.length; i++) {
var f = features[i];
var type = f && f.properties && f.properties.type;
if ('poi,text,arrow,circle,ellipse,path,rect'.split(',').indexOf(type) < 0)
continue;
switch (type) {
case 'rect':
L.search_draw.rectangle(f).addTo(this);
break;
case 'path':
L.search_draw.path(f).addTo(this);
break;
case 'poi':
L.search_draw.poi(f).addTo(this);
break;
case 'text':
L.search_draw.text(f).addTo(this);
break;
case 'arrow':
L.search_draw.arrow(f).addTo(this);
break;
case 'ellipse':
case 'circle':
L.search_draw.ellipse(f).addTo(this);
break;
default:
console.log('unknown', f);
}
}
},
});
L.search_draw.drawingPane = function() {
return new L.search_draw.DrawingPane(arguments);
};
}());
function _swiss2wgs(x, y) {
var xs = (x - 600000.0) / 1000000.0;
var ys = (y - 200000.0) / 1000000.0;
var l = 2.6779094 + 4.728982 * xs + 0.791484 * xs * ys + 0.1306 * xs * ys * ys - 0.0436 * xs * xs * xs;
var p = 16.9023892 + 3.238272 * ys - 0.270978 * xs * xs - 0.002528 * ys * ys - 0.0447 * xs * xs * ys - 0.0140 * ys * ys * ys;
return [
p * 100 / 36,
l * 100 / 36 
];
}
function _wgs2swiss(latlng) {
var lams = (latlng.lng * 3600 - 26782.5) / 10000;
var lams2 = lams * lams;
var lams3 = lams2 * lams;
var phis = (latlng.lat * 3600 - 169028.66) / 10000;
var phis2 = phis * phis;
var phis3 = phis2 * phis;
return [
Math.round(600072.37 + 211455.93 * lams - 10938.51 * lams * phis - 0.36 * lams * phis2 - 44.54 * lams3),
Math.round(200147.07 + 308807.95 * phis + 3745.25 * lams2 + 76.63 * phis2 - 194.56 * lams2 * phis + 119.79 * phis3)
];
}
function _poi_px_distance_to(poi, latlng, map)
{
if (typeof latlng == "undefined")
return Infinity;
var poi_point   = map.latLngToLayerPoint(poi.latlng ? poi.latlng : L.latLng(poi.lat, poi.lon));
var mouse_point = map.latLngToLayerPoint(latlng);
if (poi.dyn) {
var x = mouse_point.x;
var y = mouse_point.y;
var bbox = [
poi_point.x - poi.offsetx,
poi_point.y - poi.offsety,
poi_point.x + (poi.iwidth - poi.offsetx),
poi_point.y + (poi.iheight - poi.offsety)
];
var insideXbounds = x >= bbox[0] && x <= bbox[2];
var insideYbounds = y >= bbox[1] && y <= bbox[3];
if (insideXbounds && insideYbounds)
return 0;
if (insideXbounds)
return y <= bbox[1] ? bbox[1] - y : y - bbox[3];
if (insideYbounds)
return x <= bbox[0] ? bbox[0] - x : x - bbox[2];
return Math.min(
mouse_point.distanceTo(L.point(bbox[0], bbox[1])),
mouse_point.distanceTo(L.point(bbox[0], bbox[3])),
mouse_point.distanceTo(L.point(bbox[2], bbox[1])),
mouse_point.distanceTo(L.point(bbox[2], bbox[3]))
);
} else {
return mouse_point.distanceTo(poi_point);
}
}
window.SearchChJquery = $;
S = {};
S.Leaflet = L.Class.extend({
initialize: function (p)
{
var self = this;
var env = this.env = p.env;
var mapcontainer = this.mapcontainer = p.mapcontainer;
this.dynpois = {};
this._envRequestCount = 0;
this._lastEnvRequest = -1;
this.frontend_zoom_offset = 1;
this.minZoom = env.min_zoom;
this.maxZoom = env.max_zoom;
this.infocard = $(mapcontainer).find('.map-info-card');
if (!this.infocard.length)
this.infocard = $(mapcontainer).next().find('.map-info-card');
if (!this.infocard.length)
this.infocard = $('.map-info-card');
var marker_offset = [env.center.lat, env.center.lon];
var initial_center = (env.center.base_type == "request") ? $.extend({}, env.center) : this.dynamic_center(env.center);
if (env.center.pos) {
initial_center.lon = env.center.pos.lon;
initial_center.lat = env.center.pos.lat;
} else if (env.center.base_type == "telcloud" && env.geo_position && this.lz2zd(initial_center.lz) >= 512) {
initial_center.lz = 10;
initial_center.lon = env.geo_position.coords.longitude;
initial_center.lat = env.geo_position.coords.latitude;
}
var map_bounds = [[45.18624,11.457574],[48.389974,5.023348]];
var map = this.map = L.map(mapcontainer, {
crs: L.CRS.EPSG3857,
zoom: initial_center.lz + this.frontend_zoom_offset,
zoomControl:false,
fadeAnimation:false,
bounceAtZoomLimits:true,
attributionControl:false,
search_contextmenu:true,
search_poi_click:true,
search_poi_hover:env.ua.full,
maxBounds: map_bounds,
minZoom: this.minZoom + this.frontend_zoom_offset,
maxZoom: this.maxZoom + this.frontend_zoom_offset,
gestureHandling: p.gestureHandling,
gestureHandlingOptions: {
text:p.gestureHandlingText,
duration: 1000
}
});
if ('setActiveArea' in map) {
var offsets = this.offsets();
map.setActiveArea({position:'absolute', 'top':offsets.top + 'px', bottom:offsets.bottom + 'px', left:offsets.left + 'px', right:offsets.right + 'px'});
}
map._frontend_zoom_offset = this.frontend_zoom_offset;
map.setView([initial_center.lat, initial_center.lon], initial_center.lz + this.frontend_zoom_offset);
var pixelratio = window.devicePixelRatio ? window.devicePixelRatio : 1;
var drawing =  env.center.drawing;
var poi_drawing = this.infocard.find('.sl-card-inner').data('drawing');
var tileoptions = {
mapserver: env.map_server,
tileSize: env.ua.full ? 512 : 256,
noWrap: true,
continuousWorld: true,
bounds: map_bounds,
poiadmin: env.poiadmin ? '&poiadmin=' + $.now() : '',
maxZoom: this.maxZoom + this.frontend_zoom_offset
};
var bgoptions = $.extend({
zIndex:2,
layer:env.layer || (env.type == "low" ? "sym" : "bg"),
}, tileoptions);
var poi_drawing_options = $.extend({
zIndex:3,
pixelratio:pixelratio,
drawing: poi_drawing
}, tileoptions);
var fgoptions = $.extend({
zIndex:4,
layer:env.type == "low" ? "fg" : "fgbg",
pixelratio:pixelratio,
telparams:'',
poilayers:''
}, tileoptions);
this.poi = env.poi ? env.poi : 'default' + (env.center.additional_poi ? env.center.additional_poi : '');
this.poi_sticky = !!env.poi;
this.layer = env.center.layer;
this.update_fglayer(fgoptions);
var bglayer = this.bglayer = L.tileLayer('{mapserver}/chmap.jpg?layer={layer}&l={tileSize}:{z}/{y}/{x}', bgoptions).addTo(map);
this.poi_drawing_layer = L.tileLayer('{mapserver}/chmap.png?layer=draw&l={tileSize}:{z}/{y}/{x}&pixelratio={pixelratio}&d={drawing}', poi_drawing_options);
this.set_poi_drawing(poi_drawing);
var fglayer = this.fglayer = L.tileLayer('{mapserver}/chmap.png?layer={drawingsBelowText}{layer}{poilayers}{drawingsAboveText}&l={tileSize}:{z}/{y}/{x}&poi={poi}{telparams}&pixelratio={pixelratio}{drawing}{route}{poiadmin}',fgoptions).addTo(map);
if (env.center.route && 'setRoute' in map)
map.setRoute(env, this);
var markerlayer = this.markerlayer = L.marker(marker_offset, {
icon:L.divIcon({
iconSize:[20, 30],
iconAnchor:[10, 30],
className:'',
html:'<span class="sl-icon-service-map"></span>'
}),
interactive: false,
zIndexOffset:200
});
if (env.center.marker) {
if (typeof env.center.marker == 'object')
markerlayer.setLatLng(env.center.marker);
markerlayer.addTo(map);
}
L.control.attribution({ position:'bottomleft', prefix:env.copyright }).addTo(map);
L.Control.Type = L.Control.extend({
options: {
position: 'topright',
small: false
},
initialize: function (options) {
L.Util.setOptions(this, options);
},
onAdd: function (map) {
var container = L.DomUtil.create('div', 'map-control-mode-container ' + (this.options.small ? 'leaflet-bar' : 'sl-card-inner') + (env.type == "low" ? "" : " map-mode-bg"));
if (this.options.small) {
this.sym = L.DomUtil.create('a', 'map-control-mode-sym sl-off-card-title', container);
this.bg  = L.DomUtil.create('a', 'map-control-mode-bg  sl-off-card-title', container);
this.sym.href = '#';
this.bg.href = '#';
L.DomEvent
.on(this.sym, 'click', this._switch_to_sym, this)
.on(this.sym, 'click', this._refocusOnMap, this)
.on(this.sym, 'click', this._prevent_default, this)
.on(this.bg,  'click', this._switch_to_bg, this)
.on(this.bg,  'click', this._refocusOnMap, this)
.on(this.bg,  'click', this._prevent_default, this);
} else {
this.a = L.DomUtil.create('a', 'map-control-mode', container);
this.sym = L.DomUtil.create('span', 'map-control-mode-sym sl-off-card-title', this.a);
this.bg  = L.DomUtil.create('span', 'map-control-mode-bg  sl-off-card-title', this.a);
L.DomEvent
.on(this.a, 'click', this._toggle_type, this)
.on(this.a, 'click', this._refocusOnMap, this)
.on(this.a, 'click', this._prevent_default, this);
}
this.sym.innerHTML = self.env.T.map_bandwidth_low;
this.bg.innerHTML = self.env.T.map_bandwidth_high;
L.DomEvent
.disableScrollPropagation(container)
.disableClickPropagation(container);
return container;
},
onRemove: function (map) {
if (this.options.small)
L.DomEvent
.off(this.sym, 'click', this._switch_to_sym, this)
.off(this.sym, 'click', this._refocusOnMap, this)
.off(this.sym, 'click', this._prevent_default, this)
.off(this.bg,  'click', this._switch_to_bg, this)
.off(this.bg,  'click', this._refocusOnMap, this)
.off(this.bg,  'click', this._prevent_default, this);
else
L.DomEvent
.off(this.a, 'click', this._toggle_type, this)
.off(this.a, 'click', this._refocusOnMap, this)
.off(this.a, 'click', this._prevent_default, this);
},
_toggle_type: function () {
self.mode_switch(bglayer.options.layer == "bg" ? "low" : "high");
},
_switch_to_bg: function () {
if (env.type != 'high')
self.mode_switch("high");
},
_switch_to_sym: function () {
if (env.type != 'low')
self.mode_switch("low");
},
_prevent_default: function(e) {
e.preventDefault();
}
});
L.control.type = function(id, options) {
return new L.Control.Type(id, options);
};
this.controls = {
ruler: L.control.scale({ position:'bottomright', metric:true, imperial:false, maxWidth:150 }),
zoom:  L.control.zoom( { position:'bottomright', zoomInTitle:env.T.zoomin, zoomOutTitle:env.T.zoomout }),
type:  L.control.type( { position:'topright', small:true })
};
if (env.ua.full) {
map.on("zoomanim", function(e) {
map.removeLayer(fglayer);
fglayer.setZIndex(1);
});
fglayer.on("load", function() {
fglayer.setZIndex(4);
});
}
map.on("zoomend", function() {
if (env.ua.full)
map.addLayer(this.fglayer);
var zd = this.get_zd();
$.each(this.dynpois, function(id, poi) {
if (poi.dyn) {
poi.zoomvisible = zd <= poi.maxzoom && zd >= poi.minzoom;
if (poi._updateVisibility)
poi._updateVisibility();
}
});
}.bind(this));
map.on("redrawall", function() {
fglayer.redraw();
bglayer.redraw();
});
var poi_click_handler = L.Handler.extend({
addHooks: function () {
this._dblclicktimestamp = 0;
if (this._map) {
this._map.on('dblclick', this._set_dblclicktimestamp, this);
this._map.on('click', this._load_poi, this);
$(window).on('sl_statechange_poi_id', this._on_statechange.bind(this));
if (this._markerlayer_removed)
this._map.addLayer(markerlayer);
}
},
removeHooks: function () {
if (this._map) {
this._map.off('dblclick', this._set_dblclicktimestamp, this);
this._map.off('click', this._load_poi, this);
$(window).off('sl_statechange_poi_id', this._on_statechange.bind(this));
if (this._map.hasLayer(markerlayer)) {
this._map.removeLayer(markerlayer);
this._markerlayer_removed = true;
}
}
self.infocard.slideUp('fast');
self.update_poi_state('');
},
_set_dblclicktimestamp: function () {
this._dblclicktimestamp = new Date().getTime();
},
_load_poi: function (e) {
if (this._map.coordinate_marker)
this._map.coordinate_marker.remove();
var timestamp = new Date().getTime();
var zd = self.get_zd();
var url;
if (self.editpoi_url) {
$(this.mapcontainer).css('cursor', 'wait');
var coord = _wgs2swiss(e.latlng);
url = LU(self.editpoi_url, {
'mpoi_x': coord[0],
'mpoi_y': coord[1],
'save': 1
});
var finish = function() {
fglayer.options.poiadmin = '&poiadmin=' + $.now();
fglayer.redraw();
$(self.mapcontainer).css('cursor', 'grab');
self.editpoi_url = null;
}.bind(this);
$.ajax({
url: url,
success: finish,
error: function() {
finish();
alert('some error occured while saving new position');
}
});
return;
}
url = LU(self.env.map_server + '/api/poi_html.json', {
lon: e.latlng.lng,
lat: e.latlng.lat,
zd:  zd,
poi: self.poi.replace(/default/, env.poi_defaults[self.get_zoom()]),
d:   env.center.drawing,
lang: self.env.language,
layer: self.layer,
geodbpois: self.env.geodbpois
}) + (env.center.telparams ? env.center.telparams : '') + fglayer.options.poiadmin;
var washidden = self.infocard.is(":hidden");
self.infocard.slideUp('fast');
self.update_poi_state("");
self.poi_drawing_layer.removeFrom(self.map);
$("#map-overlay-left").animate({scrollTop:0});
var activeDynpois = [];
$.each(self.dynpois, function(id, poi) {
var active = poi.enabled && poi.visible && poi.zoomvisible;
poi.dist = active ? _poi_px_distance_to(poi, e.latlng, self.map) : 0;
if (active && poi.dist < 5) {
activeDynpois.push(poi);
if (poi.events) {
poi.triggerEvent('click', e);
poi.triggerEvent('mouseclick', $.extend({}, e, {type:'mouseclick'}));
poi._trigger_popupopen(e);
}
} else if (poi.events) {
poi._trigger_popupclose(e);
}
});
$.getJSON(url, function(data) {
var dblclickthreshold = 300;
if (Math.abs(timestamp - this._dblclicktimestamp) > dblclickthreshold)
{
setTimeout(function() {
if (Math.abs(timestamp - this._dblclicktimestamp) < dblclickthreshold)
return;
if (markerlayer.poi)
this._map.removeLayer(markerlayer);
if (activeDynpois.length || (data.pois && (!data.address || washidden)))
{
if (activeDynpois.length) {
activeDynpois = activeDynpois.sort(function(a, b) { return a.dist - b.dist; });
data.coords = activeDynpois[0].latlng;
data.lat = activeDynpois[0].lat;
data.pois = $.map(activeDynpois, function(poi, i) {
return poi._card ? poi._card() : (i ? poi.card : poi.card.replace(/sl-card-body/, 'sl-card-body active'));
}).join('') + (data.pois && !data.address ? data.pois : '');
data.dynpoi_id = activeDynpois[0].id;
}
self._update_poi_card(data);
self.update_poi_state(data.poi_id);
}
}.bind(this), Math.max(0, dblclickthreshold - (new Date().getTime() - timestamp)) );
}
}.bind(this));
},
_on_statechange: function (e, newid) {
if (!newid)
return;
var url = LU('/api/poi_html.json', {poi_id: newid});
self.infocard.slideUp('fast');
$.getJSON(url, self._update_poi_card.bind(self));
}
});
map.addHandler('search_poi_click', poi_click_handler);
if (env.ua.full) {
var poi_hover_handler = L.Handler.extend({
initialize: function (map) {
L.Handler.prototype.initialize.call(this, map);
this.hover_latlng = undefined;
this.hover_slack = undefined;
this.hover_cache = {};
this.hover_requested = {};
this.hover_popup = L.popup({
closeButton: false,
autoPan: false,
maxWidth: 400,
closeOnClick: false,
offset: [0, 2]
});
},
addHooks: function () {
if (this._map)
this._map.on("mousemove", this._onmousemove, this);
},
removeHooks: function () {
if (this._map) {
this._map.off("mousemove", this._onmousemove, this);
this._close();
}
},
_close: function () {
this._map.closePopup(this.hover_popup);
this.hover_popup.setContent('');
},
_show: function (data) {
var pois = data.pois;
var zd = self.get_zd();
var disp_pois = [];
var consider = function(poi) {
var dist = _poi_px_distance_to(poi, this.hover_latlng, this._map);
if (dist <= (poi.dyn ? 5 : this.hover_slack)) {
poi.dist = dist;
disp_pois.push(poi);
}
}.bind(this);
$.each(self.dynpois, function(id, poi) {
if (poi.enabled && poi.visible && poi.zoomvisible)
consider(poi);
});
if (pois)
for (var i = 0; i < pois.length; i++)
consider(pois[i]);
if (disp_pois.length) {
disp_pois = disp_pois.sort(function(a, b) { return a.dist - b.dist; });
var html = '<table class="map-poi-hover">' + $.map(disp_pois, function(poi) {
return '<tr><td>' + (poi.icon ? '<img src="' + (poi.dyn ? poi.icon : self.env.map_server + '/images/icons/' + poi.icon) + '" width=17 height=17>' : '') + '</td><td><span class="title">' + (poi.title ? poi.title : poi.html) + '</span> <span class="name">' + poi.name + '</span></td></tr>';
}).join('') + '</table><div class="map-poi-hover-tipp">' + env.T.poi_hover_tipp + '</div>';
if (html != this.hover_popup._content)
this.hover_popup.setLatLng(
disp_pois[0].lat ? [disp_pois[0].lat, disp_pois[0].lon] : (
disp_pois[0].x ? _swiss2wgs(disp_pois[0].x, disp_pois[0].y)
: disp_pois[0].latlng
))
.setContent(html)
.openOn(this._map);
$(mapcontainer).css('cursor', 'pointer');
return;
}
$(mapcontainer).css('cursor', '');
this._close();
},
_onmousemove: function (e) {
if ($(e.originalEvent.target).closest('.map-info-card-container, .leaflet-control-zoom, .map-control-mode-container, .route-marker, .map-stop-propagation').length) {
this._close();
return true;
}
if (e.originalEvent.buttons)
return true;
var poi = self.poi.replace(/default/, env.poi_defaults[self.get_zoom()]);
if ($.isEmptyObject(self.dynpois) && !env.center.drawing && (poi == '' || poi == '-') && !env.center.telparams && !self.layer)
return true;
var zd = self.get_zd();
var grid_px = 128;
this.hover_slack = 12;
var lon = e.latlng.lng;
var lat = e.latlng.lat;
var zoom = self.get_zoom();
var coords_px = self.map.options.crs.latLngToPoint(e.latlng, self.map.getZoom());
this.hover_latlng = new L.LatLng(e.latlng.lat, e.latlng.lng);
var url = LU(self.env.map_server + '/api/poi_html.json', {
mode: 'hover',
bbox_px: [
Math.floor(coords_px.x / grid_px) * grid_px - this.hover_slack,
Math.floor(coords_px.y / grid_px) * grid_px - this.hover_slack,
Math.ceil( coords_px.x / grid_px) * grid_px + this.hover_slack,
Math.ceil( coords_px.y / grid_px) * grid_px + this.hover_slack,
],
zd:   zd,
poi:  poi,
d:    env.center.drawing,
lang: self.env.language,
layer: self.layer
}) + (env.center.telparams ? env.center.telparams : '') + fglayer.options.poiadmin;
var data = this.hover_cache[url];
if (data) {
this._show(data);
} else if (!this.hover_requested[url]) {
this.hover_requested[url] = true;
$.getJSON(url, function(data) {
this.hover_cache[url] = data;
this._show(data);
}.bind(this));
} else {
this._show({});
}
}
});
map.addHandler('search_poi_hover', poi_hover_handler);
}
$('.map-stop-propagation').each(function() {
L.DomEvent.disableScrollPropagation(this).disableClickPropagation(this);
});
},
offsets: function()
{
return {top:0, bottom:0, left:0, right:0};
},
update_poi_state: function() {},
zd2lz: function(zd)
{
return 16 - Math.round(Math.log(zd) / Math.log(2));
},
lz2zd: function(lz)
{
return Math.pow(2, 16 - lz);
},
zoom2exactmpp: function(zoom, lat)
{
if (lat === undefined)
lat = 46.801201;
return 2 * Math.PI * 6378137 * Math.cos(lat * Math.PI / 180) / (512 * Math.pow(2, zoom));
},
get_zd: function()
{
return this.lz2zd(this.get_zoom());
},
set_zd: function(zd)
{
this.set_zoom(this.zd2lz(zd));
},
set_zoom: function(zoom)
{
this.map.setZoom(zoom + this.frontend_zoom_offset);
},
get_zoom: function()
{
return this.map.getZoom() - this.frontend_zoom_offset;
},
get_url_params: function() {
var c = this.map.getCenter();
var xy = _wgs2swiss(c);
var result = {
x: '',
y: '',
pos: Math.round(xy[0]) + ',' + Math.round(xy[1]),
zoom: this.get_zoom()
};
return result;
},
dynamic_center: function (center)
{
var env = this.env;
center = $.extend({}, center);
if (center.dimensions) {
var div_width  = $(this.mapcontainer).innerWidth();
var div_height = $(this.mapcontainer).innerHeight();
var offsets = this.offsets();
if (center.base_type == "default") {
div_height -= offsets.top;
center.lz = this.maxZoom;
for (var lz = this.maxZoom; lz >= this.minZoom; lz--) {
var exactmpp = this.zoom2exactmpp(lz);
var width_fits  = center.dimensions[0] >= div_width * exactmpp;
var height_fits = center.dimensions[1] >= div_height * exactmpp;
if (width_fits && height_fits)
center.lz = lz;
}
} else {
var offsets = this.offsets();
var slack_px = 2 * (center.slack_px || 10);
div_width  -= offsets.left + offsets.right + slack_px;
div_height -= offsets.top + offsets.bottom + slack_px;
center.lz = this.minZoom;
for (var lz = this.minZoom; lz <= (center.centerpois_max_zoom || this.maxZoom); lz++) {
var exactmpp = this.zoom2exactmpp(lz);
var width_fits  = center.dimensions[0] <= div_width * exactmpp;
var height_fits = center.dimensions[1] <= div_height * exactmpp;
if (width_fits && height_fits)
center.lz = lz;
}
}
}
if (center.dxm)
center.x += parseInt(center.dxm);
if (center.dym)
center.y += parseInt(center.dym);
if (center.lz)
center.lz = parseInt(center.lz);
latlon = _swiss2wgs(center.x, center.y);
center.lat = latlon[0];
center.lon = latlon[1];
return center;
},
stateupdate: function(p)
{
if (!p.center)
return;
this.env.center = p.center;
if (!this.poi_sticky)
this.poi = "default" + (p.center.additional_poi ? p.center.additional_poi : '');
if (!p.noRouteUpdate && 'setRoute' in this.map)
this.map.setRoute(this.env, this);
if (!p.noFgUpdate)
this.update_fglayer();
this.stateupdate_ui(p);
if (!p.noCenterUpdate)
this.setCenter(p.center);
else
this.setMarker(p.center);
},
stateupdate_ui: function(p) {},
setCenter: function(center, p)
{
var dyncenter = this.dynamic_center(center);
this.map.setView([dyncenter.lat, dyncenter.lon], dyncenter.lz + this.frontend_zoom_offset, p || { animate:false });
this.setMarker(center);
},
setMarker: function(center)
{
if (center.marker)
this.markerlayer.setLatLng([center.lat, center.lon]).addTo(this.map);
else
this.map.removeLayer(this.markerlayer);
},
update_fglayer: function(inoptions)
{
var env = this.env;
var drawing = env.center.drawing;
var options = typeof inoptions == "undefined" ? this.fglayer.options : inoptions;
var drawingsBelowText = '';
if (env.center.drawingsBelowText)
drawingsBelowText = env.center.drawingsBelowText + ",";
options.route = (env.center.route && !('setRoute' in this.map)) ? '&route=' + JSON.stringify(env.center.route.params) : '';
options.drawing = env.center.drawing && !('drawing_objects' in env.center) && !('drawing_geojson' in env.center) ? '&d=' + env.center.drawing : '';
options.drawingsBelowText = drawingsBelowText,
options.drawingsAboveText = (drawingsBelowText.indexOf('draw') < 0 ? ',draw' : '') + (drawingsBelowText.indexOf('pois') < 0 ? ',pois' : '');
options.telparams = env.center.telparams ? env.center.telparams : '';
options.poi = this.poi;
options.poilayers = this.layer ? ',' + this.layer: '';
if (typeof inoptions == "undefined")
this.fglayer.redraw();
},
mode_switch: function(mode)
{
var photolayer = mode == "high";
$(this.mapcontainer).find('.map-control-mode-container').toggleClass("map-mode-bg", photolayer);
this.bglayer.options.layer  = photolayer ? "bg"   : "sym";
this.fglayer.options.layer  = photolayer ? "fgbg" : "fg";
this.bglayer.redraw();
this.fglayer.redraw();
var oldtype = this.env.type;
this.env.type = photolayer ? 'high' : 'low';
if (oldtype != this.env.type)
this.map.fire('maptypechanged', { oldtype:oldtype, newtype:this.env.type });
},
set_controls: function(controls)
{
$.each(this.controls, function(key, control) {
var active = !!control._map;
if (!active && controls[key]) {
if (typeof controls[key] == 'string')
control.setPosition(controls[key]);
control.addTo(this.map);
} else if (active && !controls[key]) {
control.remove();
}
}.bind(this));
},
add_poi: function(poi)
{
var defaults = {
dyn: true,
iwidth: 21,
iheight: 21,
offsetx: 10,
offsety: 10,
visible: true,
enabled: true,
};
for (var k in defaults)
if (!(k in poi))
poi[k] = defaults[k];
poi.map = this.map;
if (!poi.latlng)
poi.latlng = L.latLng(poi);
poi.layer = L.marker(poi.latlng, {
draggable: poi.draggable,
icon:L.icon({
iconUrl:poi.icon,
iconSize:[poi.iwidth, poi.iheight],
iconAnchor:[poi.offsetx, poi.offsety]
}),
keyboard:false
});
if (!poi.maxzoom)
poi.maxzoom = this.lz2zd(this.minZoom);
if (!poi.minzoom)
poi.minzoom = this.lz2zd(this.maxZoom);
var zd = this.get_zd();
poi.zoomvisible = zd <= poi.maxzoom && zd >= poi.minzoom;
if (poi.visible && poi.zoomvisible)
poi.layer.addTo(this.map);
poi.layer.on('mouseover mouseout', function(e) {
if ('triggerEvent' in poi)
poi.triggerEvent(e.type, e);
});
if (poi.draggable) {
poi.layer.on('dragstart dragend drag', function(e) {
if (!e.latlng)
e.latlng = poi.layer._latlng;
poi.latlng = e.latlng;
poi.lat = e.latlng.lat;
poi.lon = e.latlng.lng;
poi.p.center = [poi.latlng.lng, poi.latlng.lat];
var hover_popup = this.map.search_poi_hover && this.map.search_poi_hover.hover_popup;
if (hover_popup)
hover_popup.setLatLng(poi.latlng);
if (this.markerlayer && this.markerlayer.poi && this.markerlayer.poi.dynpoi_id == poi.id)
this.markerlayer.setLatLng(poi.latlng);
if ('triggerEvent' in poi)
poi.triggerEvent(e.type, e);
}.bind(this));
}
this.dynpois[poi.id] = poi;
return poi;
},
remove_poi: function(poi)
{
this.map.removeLayer(poi.layer);
delete this.dynpois[poi.id];
},
wgs2swiss: function(lon, lat)
{
if (lon > 20) { 
var tmp = lon;
lon = lat;
lat = tmp;
}
return _wgs2swiss({lng:lon, lat:lat});
},
get_center: function(params, callback)
{
var curReq = ++this._envRequestCount;
$.getJSON(
U(this.env.map_server + "/api/getcenter.json", {'lang': this.env.lang}),
params,
L.bind(function(center) {
if (curReq > this._lastEnvRequest) {
this._lastEnvRequest = curReq;
callback(center);
}
}, this)
);
},
set_poi_drawing: function(poi_drawing)
{
if (poi_drawing) {
this.poi_drawing_layer.options.drawing = poi_drawing;
this.poi_drawing_layer.addTo(this.map);
this.poi_drawing_layer.redraw();
} else {
this.poi_drawing_layer.removeFrom(this.map);
}
},
geoJsonLayer: function(geojson, options) {
var result = L.geoJson(
geojson,
$.extend(
{},
{
pointToLayer: function (feature, latlng) {
return L.circleMarker(latlng, {pane: "markerPane", fillOpacity: 1.0});
},
onEachFeature: function (feature, layer) {
layer.on('mouseover', function () {
var style = {'color': layer.getPopup().isOpen() ? '#ff0000' : '#0000ff'};
this.setStyle(style);
if ('getArrowheads' in this)
this.getArrowheads().invoke('setStyle', style);
});
layer.on('mouseout', function () {
var style = {'color': layer.getPopup().isOpen() ? '#ff0000' : '#3388ff'};
this.setStyle(style);
if ('getArrowheads' in this)
this.getArrowheads().invoke('setStyle', style);
});
layer.on("popupopen", function () {
var style = {'color': '#ff0000'};
this.setStyle(style);
if ('getArrowheads' in this)
this.getArrowheads().invoke('setStyle', style);
});
layer.on("popupclose", function () {
var style = {'color': '#3388ff'};
this.setStyle(style);
if ('getArrowheads' in this)
this.getArrowheads().invoke('setStyle', style);
});
if (feature.properties) {
var properties = Object.assign({}, feature.properties);
var url = properties.url;
delete properties.url;
layer.bindPopup('<pre>' + (url ? '<a href="' + url + '">' : '') + feature.id + (url ? '</a>' : '') + "\n" +JSON.stringify(properties,null,' ')+'</pre>', {maxWidth: 600});
}
},
arrowheads: {frequency: 'endonly', size: '20%'}
},
options
)
);
if (!options || !('style' in options)) {
var that = this;
var update = function () {
zd = that.get_zd();
result.invoke('setStyle', {'weight': Math.min(10, Math.max(1, 2 / zd)), 'radius': Math.min(5, Math.max(0.5, 4 / zd))});
};
update();
this.map.on("zoomend", update);
}
return result;
},
_update_poi_card: function (data) {
this.markerlayer.poi = data;
this.markerlayer.setLatLng(data.coords).addTo(this.map);
this.infocard.html(data.pois).removeClass('sl-collapsed').slideDown('fast');
this.set_poi_drawing(this.infocard.find('.sl-card-inner').data('drawing'));
this.map.fireEvent('map-info-card-update');
},
_swiss2wgs: _swiss2wgs,
_wgs2swiss: _wgs2swiss
});
L.Map.include({
_getBoundsOffset: function (pxBounds, maxBounds, zoom) {
var projectedMaxBounds = L.bounds(
this.project(maxBounds.getNorthEast(), zoom),
this.project(maxBounds.getSouthWest(), zoom)
),
slop = pxBounds.getSize().divideBy(2),
minOffset = projectedMaxBounds.min.subtract(pxBounds.min).subtract(slop),
maxOffset = projectedMaxBounds.max.subtract(pxBounds.max).add(slop),
dx = this._rebound(minOffset.x, -maxOffset.x),
dy = this._rebound(minOffset.y, -maxOffset.y);
return new L.Point(dx, dy);
}
});
function parse_point(point) {
if (point.x) {
return {x: parseInt(point.x), y: parseInt(point.y)};
} else {
var splitted = point.split(',');
return {x: parseInt(splitted[0]), y: parseInt(splitted[1])};
}
}
S.Map = S.Leaflet.extend({
initialize: function(options) {
window.SearchChJquery = $;
window.searchmap = this;
this.resize();
$(window).on("resize", this.resize.bind(this));
$(window).on("orientationchange", function() { $(window).scrollTop(0) });
$("body").toggleClass("map-mode-bg", env.type != "low");
S.Leaflet.prototype.initialize.call(this, {
mapcontainer: document.getElementById('mapcontainer'),
env: $.extend({geodbpois:1}, window.env)
});
var map = this.map;
this.set_controls(options.controls);
map.addHandler('search_contextmenu', S.Map.ContextmenuHandler);
this.geomarker = L.marker([0,0], { icon:L.divIcon({ className: 'map-marker-geo map-watch', iconSize:[16, 16], html:'<div class="map-marker-geo-arrow map-arrow-watch">▲</div>' }) });
this.geoaccuracymarker = L.circleMarker([0,0], { color:'#136AEC', fillColor:'#136AEC', fillOpacity:0.15, weight:2, opacity:0.5 });
if (env.geojson)
this.geoJsonLayer(env.geojson).addTo(map);
map.on('moveend viewreset', function(e) {
$('#mapcontainer').trigger('statechange');
});
search_state_modify({'_center': env.center}, {trigger_events: false});
$('#mapcontainer').on('statechange', function(e, params) {
var pos;
if (params && params.urlchange) {
var state = search_split_url(U(env.center.base_url));
state._center = env.center;
pos = env.center.x + "," + env.center.y;
$('.input-near').val(pos);
map.stateChanged = true;
if (state.time_type == 'depart')
delete state.time_type;
search_state_modify(state, {trigger_events: false, replace: true, history: params && params.history});
search_state_modify({_settings: 'close'}, {});
} else {
var p = this.get_url_params();
pos = p.pos;
if (p.pos == env.center.x + ',' + env.center.y)
p.pos = '';
$('.input-near').val(pos);
map.stateChanged = true;
search_state_modify(p, {trigger_events: false});
}
map.fire('search_draw:changed');
}.bind(this));
$(window).on('sl_statechange', function (ev, oldstate, newstate) {
if (newstate._center && $.param(newstate._center) != $.param(oldstate._center)) {
this.stateupdate({center:newstate._center, noCenterUpdate: newstate.pos && newstate.z});
$('#map-form-mode .sl-toggle-option[data-value="' + newstate._center.mode + '"]').trigger('click');
$('.map-route-summary.active').removeClass('active');
$('.map-route-summary[data-mode=' + newstate._center.mode + ']').addClass('active');
}
if (newstate.pos != oldstate.pos || newstate.z != oldstate.z) {
var mapcenter = this.map.getCenter();
var xy = _wgs2swiss(mapcenter);
var lz = this.map.getZoom();
if (newstate.pos && newstate.pos.match(/^[\d.]+,[\d.]+/))
xy = newstate.pos.split(',');
if (newstate.z)
lz = this.zd2lz(512 / newstate.z);
map.setView(_swiss2wgs(xy[0], xy[1]), lz);
}
}.bind(this));
map.on("movestart", function(e) {
if (!map.locationMove)
{
this.geomarker.options.follow = false;
$("#map-control-locate").removeClass("sl-spinner map-control-watch");
}
map.locationMove = false;
}.bind(this));
map.on("zoomstart", function() {
map.removeLayer(this.geoaccuracymarker);
}.bind(this));
map.on("zoomend", function() {
this.geoaccuracymarker.setRadius(this.geomarker.options.accuracy / this.get_zd());
if (this.geoaccuracymarker._latlng.lat)
this.geoaccuracymarker.addTo(map);
this.update_poisettings(this.poi, this.layer);
$('#mapcontainer').trigger('statechange');
}.bind(this));
$('#map-overlay-left,#map-overlay-bottom').on('click', '*[data-center]', {}, function(e) {
var original_target = $(e.originalEvent.target);
if (original_target.is("a *,a"))
return true;
var center = $(e.currentTarget).data('center');
if (!center.route && !original_target.closest(".map-info-card .sl-icon-service-map").length)
return true;
var mode = $(e.currentTarget).data('mode');
if (mode) {
$('#map-form-mode .sl-toggle-option[data-value="' + mode + '"]').trigger('click');
} else {
$('.sl-card-body.active').removeClass('active');
$(e.currentTarget).find('.sl-card-body').addClass('active');
this.env.center = center;
this.setCenter(center, { animate:true });
this.update_poi_state($(e.currentTarget).data('poi_id'));
this.set_poi_drawing($(e.currentTarget).data('drawing'));
}
}.bind(this));
this.markerlayer.on('click', function() {
$("#map-card-bottom-content .sl-card").slideDown('fast');
});
map.on('locationfound', this._onLocationfound, this);
map.on('locationerror', this._onLocationError, this);
if (!env.ua.full) {
this.map._locateOptions = { suppressError: true };
sl_geolocate_watch_pesterfree(
L.bind(this.geolocation_watch_callback, this),
L.bind(function(watch_id) {
this._locationWatchId = watch_id;
this.startwatch(false);
sl_geolocate(function(position, error) {
if (!error)
map.fire('locationfound', position.coords);
});
}, this)
);
}
$("#map-control-locate").on('click', function() {
$(".map-warning-card").slideUp('fast');
if (!this.geomarker.options.watch)		
{
$(this).addClass("sl-spinner");
this.startwatch(true);
this.geolocation_watch();
}
else if (!this.geomarker.options.follow)	
{
this.startwatch(true);
}
else					
{
this.stopwatch();
}
}.bind(this));
$('#map_form').on('submit', function() {
map_mark_current_location_fields();
if ($('#map-address').hasClass('current-location')) {
map_fill_current_location_fields($('#map-address'), $('#map_form'));
return false;
}
return true;
});
$('.map-route-form').on('submit', function(e, p) {
var form = e.target;
$(form).find(".map-form-poi").val(this.poi_sticky ? this.poi : '');
map_mark_current_location_fields();
var current_location_fields = $(e.currentTarget).find('input.current-location:visible');
if (current_location_fields.length) {
map_fill_current_location_fields(current_location_fields, $(form));
return false;
} else if (!window.onbeforeunload && window.history && history.pushState) {
this.update_center(form, p);
return false;
}
return true;
}.bind(this));
$("#map-form-bicycle-variant input").on('change', function () {
$('.map-toggle-form:visible').trigger('submit', {noHistory: true});
});
$('#map-control-mode').on('click', function (e) {
this.mode_switch(this.bglayer.options.layer == "bg" ? "low" : "high");
}.bind(this));
$('.map-settings').on("click", ".map-setting-with-sub-settings", function(event) {
var expand = !$(this).hasClass("is-expanded");
if ($(event.target).hasClass('map-settings-checkbox'))
expand = event.target.checked || !expand;
if (!$(event.target).children('input').length && $(event.target).closest('li').is(this) && (expand != $(this).hasClass('is-expanded'))) {
if (expand && (!env.ua.full || !$(this).hasClass('map-setting-title'))) {
$(this).siblings('.map-setting-with-sub-settings').each(function (i, toclose) {
$(toclose).children('ul').slideUp(200, function () {$(toclose).removeClass('is-expanded')});
});
}
var that = this;
$(this).children("ul")[expand ? 'slideDown' : 'slideUp'](200, function () { $(that).toggleClass("is-expanded", expand)});
if (window.drawingcontroller) {
if ($('.map-drawing-tools').is(':visible'))
window.drawingcontroller.enable();
else
window.drawingcontroller.disable();
} else if ($('.map-drawing-tools').is(':visible')) {
$.getJSON(LU('/api/drawing_tools_load.json'), {id: env.center.drawing}, function (resp) {
$('#map-drawing-loading').replaceWith(resp.tools);
$('<style>' + resp.css + '</style>').appendTo('head');
$('<script>' + resp.js + '</script>').appendTo('body');
}).fail(function () {
$('#map-draw-loading').replaceWith('loading failed');
});
}
}
});
var timer = null;
$('section.map-settings').on('mouseenter maximize', function() {
clearTimeout(timer);
$(this).removeClass('minimized').animate({ 'width': '20rem' }, 50);
}).on('mouseleave', function() {
var self = $(this);
if (!self.find('.map-setting-title.is-expanded').length)
timer = setTimeout(function() { self.addClass('minimized').animate({ 'width': '2.125em' }, 50) }, 500);
});
$(".map-settings-list").on('click change', '.map-settings-checkbox', function(e) {
var li = $(e.target).closest("li");
if (li.hasClass("map-setting-with-sub-settings"))
{
li.removeClass('map-setting-partial').find('.map-settings-checkbox').prop('checked', e.target.checked);
}
else
{
var mainli = $(e.target).closest(".map-setting-with-sub-settings");
var maincb = mainli.find(".map-settings-checkbox-main");
maincb.prop('checked', mainli.find(":checked").not(maincb).length > 0);
mainli.toggleClass('map-setting-partial', mainli.find('.map-settings-checkbox').not(':checked').length > 0 && mainli.find('.map-settings-checkbox:checked').length > 0);
}
if (e.type == 'click') {
var poiparam = this.poilist();
if (poiparam == env.poi_defaults[this.get_zoom()])
poiparam = 'default';
else if (poiparam.replace(/,?(velo|wander|sommer|winter)land/g, '') == env.poi_defaults[this.get_zoom()])
poiparam = ('default,' + poiparam.match(/,?(velo|wander|sommer|winter)land/g).join(',')).replace(/,,/g, ',');
else if (poiparam == "")
poiparam = "-";
this.update_poi(poiparam, true);
this.update_layer(this.layerlist());
search_state_modify({poi: poiparam.replace(/^default$/, ''), layer: this.layer}, {trigger_events: false, persistent: true});
}
}.bind(this));
$(window).on('sl_statechange_poi', function (ev, newpoi) {
this.update_poi(newpoi, false);
$('.map-settings-checkbox').not('.map-settings-checkbox-main').prop('checked', function () {
return (this.poi.indexOf($(e.currentTarget).data('poitype')) >= 0) ||
(this.poi.indexOf($(e.currentTarget).closest('.map-setting-with-sub-settings').find('.map-settings-checkbox-main').data('poitype')) >= 0);
}.bind(this)).trigger('change');
}.bind(this));
this.update_poisettings(this.poi, this.layer);
this.map.on('map-info-card-update', function() {
$('.map-info-card .map-image-popup').fancybox({ type:'image', hash:'popup' });
}).fire('map-info-card-update');
if (env.compare_mapserver) {
var prefix = [];
prefix[0] = env.map_server.match(/map\.([\w-.]+)\.search\.ch/)[1].replace(/(\.|^)compare[^.]*\./, '$1');
prefix[1] = env.compare_mapserver.match(/map\.([\w-.]+)\.search\.ch/)[1];
L.control.attribution({ position:'bottomleft', prefix:$('<div/>', {id:'map-compare-text'})[0].outerHTML}).addTo(this.map);
$('#map-compare-text').text(prefix[0]);
var compare_bglayer = L.tileLayer(searchmap.bglayer._url, $.extend({}, searchmap.bglayer.options, {mapserver: env.compare_mapserver})).addTo(map);
var compare_fglayer = L.tileLayer(searchmap.fglayer._url, $.extend({}, searchmap.fglayer.options, {mapserver: env.compare_mapserver})).addTo(map);
var toggle_count = 0;
setInterval(function() {
var enabled = !(toggle_count & 1);
$('#map-compare-text').text(prefix[+enabled]).toggleClass('compare', enabled);
compare_bglayer.setZIndex(enabled ? 2 : 1);
compare_fglayer.setZIndex(enabled ? 4 : 1);
searchmap.fglayer.setZIndex(!enabled ? 4 : 1);
toggle_count++;
}, 2000);
}
},
resize: function() {
var height = $(window).height() - $("#mapcontainer").offset().top;
$("#mapcontainer").height(height);
var offsets = this.offsets();
$("#map-center-position").css('top', ($("#mapcontainer").innerHeight() + offsets.top - offsets.bottom) / 2);
if (this.map)
this.map.invalidateSize({});
return height;
},
mode_switch: function(mode) {
var photolayer = mode == "high";
$("body").toggleClass("map-mode-bg", photolayer);
S.Leaflet.prototype.mode_switch.call(this, mode);
$.getJSON(U('/persistent.gif?bandwidth=' + (photolayer ? 'high' : 'low') + "&t=" + new Date().getTime()));
},
update_poi: function(newpoi, force) {
this.poi_sticky = true;
this.fglayer.options.poi = this.poi = (newpoi || force) ? newpoi : 'default';
this.fglayer.redraw();
$('.input-poi').val(newpoi);
},
update_layer: function(layer) {
this.layer = layer;
this.fglayer.options.poilayers = layer ? ',' + layer: '';
this.fglayer.redraw();
$('.input-layer').val(layer);
},
update_poi_state: function(poi_id) {
search_state_modify(
$.extend(this.get_url_params(), {poi_id: poi_id}),
{trigger_events: false}
);
},
update_poisettings: function(poilist, layerlist) {
poilist = poilist.replace(/default/, env.poi_defaults[this.get_zoom()]);
var active_pois = poilist.split(/,/);
var all_active = active_pois.indexOf('all') >= 0;
$('.map-settings-checkbox').prop('checked', false);
$('.map-settings-checkbox-main').each(function() {
var root = $(this).closest('li');
root.find('.map-settings-checkbox').each(function() {
if (all_active || active_pois.indexOf($(this).data('poitype')) >= 0)
$(this).prop('checked', true).trigger('change');
});
});
if (layerlist) {
var active_layers = layerlist.split(/,/);
$(".map-settings-checkbox-direct").each(function() {
if (active_layers.indexOf($(this).data('poitype')) >= 0)
$(this).prop('checked', true).trigger('change');
});
}
},
update_route_mode: function(newmode) {
var info = $('.map-route-summary[data-mode=' + newmode + ']');
if (!env.center.drawingsBelowText)
env.center.drawingsBelowText = '';
if (info.length) {
var old = $('.map-route-summary.active');
old.find('.map-route-hide-inactive').slideUp('fast', function () {old.removeClass('active')});
$(info).find('.map-route-hide-inactive').slideDown('fast', function () {$(info).addClass('active')});
var newcenter = $.extend({route_menu: env.center.route_menu, infocard: env.center.infocard}, $(info).data('center'));
this.stateupdate({center: newcenter, noFormUpdate:true, noInfocardUpdate:true});
} else if (newmode == 'bicycle') {
this.stateupdate({center:env.center, noCenterUpdate:true, noFormUpdate:true});
this.update_fglayer();
} else {
this.stateupdate({center:env.center, noCenterUpdate:true, noFormUpdate:true});
this.update_fglayer();
}
search_state_modify({mode:newmode}, {trigger_events: false});
},
update_center: function (form, p) {
$('.ui-autocomplete.ui-menu:visible').hide();
var params = {};
var viaId = 0;
$.each($(form).serializeArray(), function(i, p) {
params[p.name == 'via[]' ? 'via[' + (viaId++) + ']' : p.name] = p.value;
});
var submit_button = $(form).find('input[type="submit"]');
submit_button.addClass('sl-spinner');
this.get_center(
params,
function (newcenter) {
submit_button.removeClass('sl-spinner');
$('#sl-service-form-button.sl-bar-button.sl-form-hidden').trigger('click');
$('#sl-service-form-container input.pressed').removeClass('pressed');
searchmap.stateupdate({center:newcenter, history: !p || !p.noHistory, noCenterUpdate: p && p.noCenterUpdate });
}
);
},
stateupdate_ui: function(p) {
env.center = p.center;
$('#mapcontainer').trigger('statechange', [{urlchange:true, history:p.history}]);
$('.map-toggle-form').data('changed', false);
if (env.center.title)
document.title = env.center.title;
if (!p.noFormUpdate) {
var route_forms = $('.map-route-form'), points;
if (env.center.route) {
points = env.center.route.points;
route_forms.find('input.sl-route-from').val(points[0]).trigger('propertychange');
} else {
points = [env.center.q];
route_forms.find('input.sl-route-from').val("").trigger('propertychange');
}
var last = points[points.length - 1];
route_forms.find('input.sl-route-to').val(last).trigger('propertychange');
route_forms.find('.sl-route-via-container').remove();
for (var i = 1; i < points.length - 1; i++)
route_forms.each(function() { sl_add_via(this, points[i])});
$('#map-address').val(last);
$('input.sl-date-input').val(env.center.date);
$('input.sl-time-input').val(env.center.time);
}
if (!p.noInfocardUpdate) {
var message_container = $(env.ua.full ? '#map-overlay-left' : '#map-overlay-top');
message_container.find('.sl-warning').remove();
if (env.center.messages)
message_container.prepend(env.center.messages);
if (p.center.infocard)
this.infocard.html(p.center.infocard).slideDown('fast');
else
this.infocard.html('').slideUp('fast');
if (this.map.coordinate_marker)
this.map.coordinate_marker.remove();
}
this.update_poisettings(this.poi, this.layer);
},
_onLocationfound: function(e) {
var map = this.map;
var latlng = new L.LatLng(e.latitude, e.longitude);
if ($("#map-control-locate").hasClass("sl-spinner"))
$("#map-control-locate").removeClass("sl-spinner").addClass("map-control-watch");
$('#sl-service-form-container input.pressed').removeClass('pressed');
if (!this.map.options.maxBounds.contains(latlng))
{
this.stopwatch();
alert(env.T.coordinates_outside_map);
return;
}
if (!map.stateChanged && (env.center.base_type == "default" || env.center.base_type == "telcloud" && this.get_zoom() < 8))
{
map.locationMove = true;
map.addOneTimeEventListener('zoomend', function() {
setTimeout(function() { map.fire('redrawall') }, 200);
});
map.setView(latlng, (env.center.base_type == "telcloud" ? 10 : 14) + this.frontend_zoom_offset);
}
else if (this.geomarker.options.follow && !map.getBounds().contains(latlng))
{
map.locationMove = true;
setTimeout(function() { map.fire('redrawall') }, 200);
map.panTo(latlng);
}
var accradius = e.accuracy / this.get_zd();
this.geomarker.options.accuracy = e.accuracy;
this.geoaccuracymarker.setLatLng(latlng).setRadius(accradius);
if (!map.touchZoom || !map.touchZoom._zooming)
this.geoaccuracymarker.addTo(map);
this.geomarker.setLatLng(latlng).addTo(map);
if ('heading' in e && e.heading >= 0)
{
var transform = 'rotate(' + e.heading + 'deg)';
$(".map-marker-geo-arrow").css({ '-webkit-transform':transform, '-moz-transform':transform, 'transform':transform }).show();
}
else
$(".map-marker-geo-arrow").hide();
},
_onLocationError: function(e) {
if (!this.geomarker.options.accuracy)
{
this.stopwatch();
if (!this.map._locateOptions.suppressError)
{
$(".map-warning-card .text").text(env.T.locationerror);
$(".map-warning-card").slideDown('fast').removeClass('sl-hidden');
}
}
},
geolocation_watch_callback: function(position, error) {
if (position)
this.map._handleGeolocationResponse(position);
else
this.map._handleGeolocationError(error);
},
geolocation_watch: function() {
if (!this._locationWatchId) {
this.map._locateOptions = {};
sl_geolocate_watch(
L.bind(this.geolocation_watch_callback, this),
L.bind(function(watch_id) {
this._locationWatchId = watch_id;
}, this)
);
}
},
startwatch: function(follow) {
$(".map-marker-geo").addClass("map-watch");
$(".map-marker-geo-arrow").addClass("map-arrow-watch");
this.geomarker.options.watch = true;
if (this.geomarker.options.follow = follow)
$("#map-control-locate").addClass("map-control-watch");
},
stopwatch: function() {
$("#map-control-locate").removeClass("sl-spinner map-control-watch");
$(".map-marker-geo").removeClass("map-watch");
$(".map-marker-geo-arrow").hide();
this.geomarker.options.watch = false;
if (this._locationWatchId) {
sl_geolocate_clear_watch(this._locationWatchId);
delete this._locationWatchId;
}
},
poilist: function() {
var pois = [];
if ($('.map-settings-poi .map-settings-checkbox').not('.map-settings-checkbox-direct').not(':checked').length == 0) {
pois.push('all');
} else {
$(".map-settings-poi .map-settings-checkbox-main").each(function() {
var lis = $(this);
if (this.checked)
{
var sublis = lis.closest("li").find(".map-settings-checkbox").not(this);
if (sublis.not(":checked").length > 0)
lis = sublis.filter(":checked");
lis.each(function() { pois.push($(this).data('poitype')); });
}
});
}
return pois.sort().join(",");
},
layerlist: function() {
var layers = [];
$(".map-settings-checkbox-direct:checked").each(function() {
$(this).each(function() { layers.push($(this).data('poitype')); });
});
return layers.sort().join(",");
},
swiss2wgs: function (swiss) {
var xs = (swiss.x - 600000) / 1000000;
var ys = (swiss.y - 200000) / 1000000;
var l = 2.6779094 + 4.728982 * xs + 0.791484 * xs * ys + 0.1306 * xs * ys * ys - 0.0436 * xs * xs * xs;
var p = 16.9023892 + 3.238272 * ys - 0.270978 * xs * xs - 0.002528 * ys * ys - 0.0447 * xs * xs * ys - 0.0140 * ys * ys * ys;
return {lat:p * 100 / 36, lon:l * 100 / 36};
}
});
S.Map.ContextmenuHandler = L.Handler.extend({
addHooks: function () {
if (this._map) {
this._map.on("contextmenu", this._showContextmenu, this);
this._map.on("click zoomstart resize movestart", this._hideContextmenu, this);
$('#mapcontainer').on('click mouseup contextmenu', '#map-contextmenu a', this._clickContextmenu.bind(this));
}
},
removeHooks: function () {
if (this._map) {
this._map.off("contextmenu", this._showContextmenu, this);
this._map.off("click zoomstart resize movestart", this._hideContextmenu, this);
$('#mapcontainer').off('click mouseup contextmenu', '#map-contextmenu a', this._clickContextmenu.bind(this));
}
this._hideContextmenu();
},
_showContextmenu: function (e) {
if ($('#map-contextmenu').is(':visible') || $(e.originalEvent.target).closest('.map-stop-propagation').length) {
this._hideContextmenu();
return true;
}
var xy = _wgs2swiss(e.latlng);
var address = xy[0] + ',' + xy[1];
var state = search_state_get();
var mode = searchmap.route_mode();
var zd = searchmap.get_zd();
var z = 512 / zd;
$('#map-contextmenu').css({
display: 'inline-block',
left: Math.min(e.containerPoint.x, $('#mapcontainer').width() - 160) + 'px',
top: e.containerPoint.y + 'px'
});
$('#map-contextmenu .map-set-from a').data({
address: address,
fromto: 'from'
}).attr('href', U('/?from=' + address + '&z=' + z));
$('#map-contextmenu .map-set-to a').data({
address: address,
fromto: 'to'
}).attr('href', U('/?to=' + address + '&z=' + z));
$('#map-contextmenu .map-add-via').toggle(mode != 'map');
$('#map-contextmenu .map-add-via a').data({
address: address,
fromto: 'via'
});
$('#map-contextmenu .map-show-coordinates a').data({
latlng: e.latlng
});
},
_hideContextmenu: function () {
$('#map-contextmenu').css({display: 'none'});
},
_clickContextmenu: function (e) {
var data = $(e.target).data();
var address = data.address;
var fromto = data.fromto;
var latlng = data.latlng;
if (e.type == 'mouseup' && e.button == 0)
return true;
if (address) {
this._hideContextmenu();
var best;
if (fromto == 'via') {
var i;
var mindist = 9999999;
var waypoints = env.center.route && env.center.route.waypoints;
if (waypoints)
for (i = 1; i < waypoints.length; i++) {
d = this.point_line_distance(waypoints[i-1], waypoints[i], address);
if (d < mindist) {
best = i - 1;
mindist = d;
}
}
}
var mode = searchmap.route_mode();
var newmode = mode == 'map' ? 'car' : mode;
var route_prefix = env.ua.full ? newmode : 'route';
var route_form = $('#' + route_prefix + '_form');
if (mode == 'map' && !$('#map-address').val()) {
window.location.href = e.currentTarget.href;
} else if (fromto == 'via') {
sl_add_via($('.map-toggle-form:visible, #route_form'), address, best);
} else {
if (mode == 'map' && fromto == 'to')
$('#' + route_prefix + '-from').val($('#map-address').val()).trigger('change');
$('#' + route_prefix + '-' + fromto).val(address).trigger('change');
if (!env.ua.full) {
$('#map_form').addClass('sl-is-hidden');
$('#route_form').removeClass('sl-is-hidden');
$("#sl-service-form-container").removeClass('sl-is-hidden');
$("#sl-service-form-button").addClass('sl-form-hidden');
}
}
$('#map-form-mode .sl-toggle-option[data-value="' + newmode + '"]').trigger('click');
if ($('#' + route_prefix + '-from').val() && $('#' + route_prefix + '-to').val())
route_form.trigger('submit');
return false;
} else if (latlng) {
this._hideContextmenu();
if (this._map.coordinate_marker)
this._map.coordinate_marker.remove();
this._map.removeLayer(searchmap.markerlayer);
var show_coordinates = function (latlng) {
var xy = _wgs2swiss(latlng);
var html = '<div class="sl-card-inner"><header><h2 class="sl-card-header-title">' +
env.T.coordinates +
'</h2></header><div class="sl-card-body"><table><tr><td rowspan="2" width="40">' +
'<img src="' + U('/images/map_coordinate_marker.svg') + '" /></td><td>' +
Math.round(xy[0]) + ', ' + Math.round(xy[1]) + '<br>' +
'</td></tr><tr><td>' +
L.Util.formatNum(latlng.lat, 5) + ', ' + L.Util.formatNum(latlng.lng, 5) +
'</td></tr><tr><td></td><td class="show-coordinates-text"><div style="height:3em" class="sl-spinner"></div></td></tr></table></div></div>';
window.searchmap.infocard.html(html).slideDown('fast');
if (this._show_coordinates_throttle) {
clearTimeout(this._show_coordinates_throttle);
this._show_coordinates_throttle = undefined;
}
if (this._show_coordinates_xhr) {
this._show_coordinates_xhr.abort();
this._show_coordinates_xhr = undefined;
}
this._show_coordinates_throttle = setTimeout(
function() {
this._show_coordinates_xhr = $.getJSON({
url: LU('/api/show_coordinates_text.json',{x:xy[0], y:xy[1]}),
success: function(res) {
window.searchmap.infocard.find('.show-coordinates-text').html(res.html);
}
});
},
50
);
}.bind(this);
show_coordinates(latlng);
this._map.coordinate_marker = L.marker(latlng, {
draggable: true,
riseOnHover:true,
icon: L.icon({
iconUrl: env.map_server + '/images/map_coordinate_marker.svg',
iconSize:    [25, 35],
iconAnchor:  [12, 35],
popupAnchor: [1, -34],
shadowSize:  [41, 41],
className: 'coordinate-marker'
})
});
this._map.coordinate_marker.on('move', function(e) {
show_coordinates(e.latlng);
});
this._map.coordinate_marker.addTo(this._map);
return false;
}
},
point_line_distance: function(fromstr, tostr, pstr) {
var from = parse_point(fromstr);
var to = parse_point(tostr);
var p = parse_point(pstr);
var dirx = to.x - from.x;
var diry = to.y - from.y;
var sqlen = dirx*dirx + diry*diry;
var t = ((p.x - from.x) * dirx + (p.y - from.y) * diry) / sqlen;
var coords;
if (t <= 0)
coords = from;
else if (t >= 1)
coords = to;
else
coords = { x:from.x + t*dirx, y:from.y + t*diry };
return Math.sqrt(Math.pow(p.x - coords.x, 2) + Math.pow(p.y - coords.y, 2));
}
});
S.Map.Full = S.Map.extend({
initialize: function(options) {
S.Map.prototype.initialize.call(this, options);
var map = this.map;
$(window).on('sl_state_update_links', function(e, oldState, newState) {
var newUrl = window.location.protocol + '//' + window.location.host + search_join_url($.extend({}, newState, {edit:''}));
$('.sl-share-native').data('url', newUrl);
$('.sl-share-options a').each(function() {
$(this).attr('href', $(this).attr('href').replace(/https?%3A%2F.*$/, window.encodeURIComponent(newUrl)));
});
});
var drawingpane = L.search_draw.drawingPane().addTo(map);
if ('drawing_geojson' in env.center)
drawingpane.readGeoJSON(env.center.drawing_geojson);
else if ('drawing_objects' in env.center)
drawingpane.readObjects(env.center.drawing_objects);
if (L.search_draw.Controller) {
window.drawingcontroller = new L.search_draw.Controller(map, drawingpane);
if ($('.map-drawing-tools').is(':visible'))
window.drawingcontroller.enable();
}
window.drawingpane = drawingpane;
this.initialize_route_menu($('.map-routeinfo'));
},
stateupdate_ui: function(p) {
if (!p.noInfocardUpdate) {
$('.map-route-menu').remove();
if (p.center.route_menu) {
var dom = $(p.center.route_menu);
$('.map-settings-list').first().prepend(dom);
sl_datetime_init(dom);
this.initialize_route_menu(dom);
$('section.map-settings').triggerHandler('maximize');
}
}
S.Map.prototype.stateupdate_ui.call(this, p);
},
initialize_route_menu: function (dom) {
dom.find('.map-route-summary').each(function() {
var dom = $(this);
var fancybox = dom.find('.map-image-popup, .map-route-start-edit').fancybox({src:dom.find('.map-image-popup-content'), type:'inline', hash:'route'});
dom.find('.map-bicycle-datetime').on('submit', function (ev) {
$('.map-toggle-form:visible').trigger('submit', {noHistory: true, noCenterUpdate: true});
ev.preventDefault();
});
dom.find('.map-bicycle-datetime').on('change', function (ev) {
var date = $(this).find('.sl-date-input').val();
var time = $(this).find('.sl-time-input').val();
$('.sl-date-input').val(date);
$('.sl-time-input').val(time);
var img = $('.map-image-popup-content').find('img');
img.attr('src', U(img.attr('src'), {date: date, time: time}));
});
if ($.fancybox.isOpen) {
$.fancybox.close();
}
});
},
resize: function() {
var height = S.Map.prototype.resize.call(this);
$('#map-overlay-left').css('max-height', height - $('#map-overlay-left').offset().top - 40);
var p = $("#mapcontainer").width() + 'x' + height;
$('.map-print-link').attr('href', $('.map-print-link').attr('href').replace(/([?&])p=\d+x\d+/, '$1p=' + p));
},
offsets: function()
{
return {
top:    $('.sl-nav-container').outerHeight() + 0,
bottom: 0,
left:   $('#map-overlay-left').outerWidth() + 0,
right:  0
};
},
route_mode: function() {
return $('#map-form-mode .sl-toggle-option.sl-is-on').data('value');
},
admin_movepoi: function(id) {
$('#mapcontainer').css('cursor', 'crosshair');
this.editpoi_url = '/editpoi/' + id;
}
});
$(function() {
new S.Map.Full({
controls: {ruler:'bottomleft', zoom:'bottomright'}
});
});
function tour_poi() {
search_state_modify({'poi':'verkehr'});
var center = {base:"600675,199640",base_url:"\/600675,199640",marker:false,lz:12,x:600675,y:199640,base_type:"poi", mode:'map'};
searchmap.setCenter(center);
}
$.fn.oev_autocomplete = function() {
return $(this).each(function() {
var is_departure_table = $(this).attr('id') == 'stop';
var is_via = $(this).attr('name') == 'via[]';
var params = {};
if (is_departure_table || is_via)
params.stoponly = 1;
if (is_via)
params.via = 1;
if ($(this).data('notel'))
params.notel = 1;
params.explicit_sortbydistance_only = 1;
$(this).sl_autocomplete({
url: env.oev_completion_url ? env.oev_completion_url : LU('/api/completion.json'),
submit_on_select: false,
request_params: params
});
});
}
function stop_spinning(elem)
{
elem.removeClass('sl-spin');
elem.hide();
setTimeout(function() {
elem.show();
}, 10);
}
$(function () {
$(".sl-form-row-action.sl-icon-search").on('click', function () {
$(this).closest("form").trigger('submit');
});
$('#sl-service-form-container form').on('submit', function(e) {
$(this).find("input[type='submit']").addClass('sl-spinner');
});
$('.oev-autocomplete.oev-departure-form').on('submit', function() {
if ($('#stop').val().trim() == env.T.sl_current_location)
$('#stop').val('');
return true;
});
$('.oev-autocomplete.geolocate_empty_field').on('submit', function() {
var current_location_fields = $(this).find('#oev-from, #oev-to').filter(function() { return !this.value.trim() || this.value.trim() == env.T.sl_current_location; });
if (current_location_fields.length) {
var form = this;
$('.geolookup-in-progress').show();
sl_geolocate(function(position, error) {
$('.geolookup-in-progress').hide();
if (!error) {
current_location_fields.val('snap:' + position.coords.latitude + ',' + position.coords.longitude);
$(form).trigger('submit');
} else {
alert(env.T.sl_locateme_error);
$('#sl-service-form-container input[type="submit"]').removeClass('sl-spinner');
}
});
return false;
}
return true;
});
$('.oev-autocomplete .sl-form-row .sl-icon-position').on('click', function() {
var input = $(this).parent().find('input');
var is_departure_table = input.attr('id') == 'stop';
var locateme_icon = $(this);
locateme_icon.addClass('sl-spin');
sl_geolocate(function(position, error) {
stop_spinning(locateme_icon);
if (!error) {
input
.autocomplete('option', 'disabled', false)
.val(env.T.sl_current_location)
.trigger('change')
.trigger('focus');
window.setTimeout ( function() {
input.trigger('autocomplete').trigger('focus');
}, 10);
} else
alert(env.T.sl_locateme_error);
});
});
$(".oev-autocomplete").on('sl_via_add', '.sl-route-via', function (ev) {
$(this).oev_autocomplete();
});
$('.oev_addvia1').on('click', function() {
$(this).hide();
$(".oev-autocomplete .oev_via").slideDown('fast').find("input").oev_autocomplete().val("").trigger('focus');
return false;
});
$('.oev-autocomplete #stop, .oev-autocomplete .sl-route-input').oev_autocomplete();
$(".oev-delete-confirm").on('submit', function() {
return confirm(env.T.oev_delete_confirm);
});
$('#oev-date-scroller, #oev-time input').on('change', function() {
if (!$(this).val() && $('.oev-mode-toggle').parent().find('input[name=time_type]').val() == 'arrival') {
$(".oev-mode-toggle .sl-toggle-a").trigger('click');
}
});
$('.oev-ticket-submenu').on('sl_collapse', function(e, data) {
$.ajax({ url:U('/persistent.gif'), data:{ ticketsubmenu_collapsed:data.collapsed ? 1 : 0 }});
})
});
timeline_mode = env.timeline;
function update_timeline(animate)
{
var legs = $(".oev-leg");
if (!legs.length)
return;
var first_triptitle = $(".oev-triptitle:visible").first();
var scale_offset = first_triptitle.find('.oev-leg').first().offset().left - first_triptitle.parents('.sl-col').first().offset().left;
var totalwidth = (first_triptitle.width() - scale_offset) * 0.8;
if (timeline_mode % 3)
$(".oev-change").hide();
$(".oev-no-scale").show();
$(".oev-durationline-scale, .oev-timeline-scale").hide();
var show_scale = (timeline_mode == 1 || timeline_mode == 2);
if ($(".oev-scale").is(':visible') != show_scale)
$(".oev-scale")[animate ? (show_scale ? 'slideDown' : 'slideUp') : (show_scale ? 'show' : 'hide')]();
var scale_width = 0;
$(".oev-leg:visible").each(function() {
var tmp_width = $(this).offset().left - scale_offset + $(this).width();
if (tmp_width > scale_width)
scale_width = tmp_width;
});
legs.each(function() {
var width = $(this).hasClass('oev-pause') ? '0' : '1.875em';
if (timeline_mode % 3) {
var meta = $(this).attr('class').match(/oev-meta-(\d+)-(\d+)/);
if (meta && (timeline_mode != 1 || (meta[1] - 0)))
width = totalwidth / (timeline_mode == 1 ? env.max_duration : env.lastend - env.firststart) * meta[2];
}
var styles = { 'min-width':width, 'margin-right':timeline_mode == 0 ? '0.313em' : '0.1em' };
if (animate)
$(this).animate(styles);
else
$(this).css(styles);
}).promise().done(function () {
if (timeline_mode == 3)
$(".oev-change").fadeIn().css('display', 'inline-block');
var total_width = 0;
var total_min_width = 0;
$('.oev-triptitle:visible .oev-leg:visible').each(function() {
total_width     += parseFloat($(this).width());
total_min_width += parseFloat($(this).css('min-width'));
});
$(".oev-durationline-scale, .oev-timeline-scale").css({ 'margin-left': scale_offset }).each(function() {
var meta = $(this).attr('class').match(/oev-meta-(\d+)/);
var width = totalwidth / (timeline_mode == 1 ? env.max_duration : env.lastend - env.firststart) * meta[1] * total_width / total_min_width;
$(this).find('td').width(width);
});
$(".oev-no-scale")[timeline_mode % 3 == 0 ? "show" : "hide"]();
$(".oev-durationline-scale")[timeline_mode == 1 ? "show" : "hide"]();
$(".oev-timeline-scale")[timeline_mode == 2 ? "show" : "hide"]();
});
if (timeline_mode == 0) {
$(".oev-change").fadeOut().promise().done(function () {$(".oev-change").hide()});
}
}
function log(key, value)
{
var params = {t: new Date().getTime()}
params[key] = value;
new Image().src = U('/persistent.gif', params);
}
function timeline_dropdown_offset(select_button)
{
var offset = select_button.offset();
offset.left -= $('body').offset().left;
if (offset.top + 4 * select_button.height() >= $(document).height())
offset.top -= 3 * select_button.height();
return offset;
}
$(function () {
update_timeline(true);
$('.oev-timeline-select > a').on('click', function() {
$(this).siblings('ul').css(timeline_dropdown_offset($(this))).slideToggle('fast');
return false;
});
$('.oev-timeline-select ul a').on('click', function() {
timeline_mode = $(this).data('timeline');
var dropdown = $(this).closest('ul');
dropdown.siblings('a').each(function() {
$(this).toggle($(this).data('timeline') == timeline_mode);
});
dropdown.find('a').removeClass('sl-icon-checked-after');
$(this).addClass('sl-icon-checked-after');
dropdown.slideUp('fast');
update_timeline(true);
search_state_modify({timeline:timeline_mode});
log('timeline_mode', timeline_mode);
return false;
});
});
$(document).on('click', function (e) {
$('.oev-timeline-select ul').slideUp('fast');
});
$('.oev-history-line').on('click', function (e) {
if (!$(e.target).is("input,a"))
document.location = $(this).find('a.sl-button').first().attr('href');
});
$(window).on('resize', function() {
$('.oev-timeline-select ul').each(function() {
$(this).css(timeline_dropdown_offset($(this).siblings('a:visible')));
});
update_timeline(false);
});
window.trace+='I';
window.it_gotjs = (window.it_gotjs ? window.it_gotjs : '') + '.oev';
